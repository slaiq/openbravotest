	<parameter name="Efin_C_Order_Report_V_ID" class="java.lang.String"/>
	<parameter name="Inv_Date_From" class="java.lang.String"/>
	<parameter name="Inv_Date_To" class="java.lang.String"/>
	<parameter name="AUXinpDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{Inv_Date_From}==null || $P{Inv_Date_From}.equals( "" )) ?  "  and 1=1   " : "    and trunc(inv.dateacct)   BETWEEN trunc(eut_convertto_gregorian( '"+$P{Inv_Date_From}+"'  )) and trunc(eut_convertto_gregorian( '"+$P{Inv_Date_To}+"'  ))     "]]></defaultValueExpression>
	</parameter>
	<parameter name="AUXinpContractID" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{Efin_C_Order_Report_V_ID}==null || $P{Efin_C_Order_Report_V_ID}.equals( "" )) ?  "    AND 1=1    " : "  and  CO.c_order_id = '"+$P{Efin_C_Order_Report_V_ID}+"'     "]]></defaultValueExpression>
	</parameter>
	<parameter name="USER_CLIENT" class="java.lang.String"/>
	<parameter name="AuxUSER_CLIENT" class="java.lang.String">
		<defaultValueExpression><![CDATA["   and co.ad_client_id  in ( "+$P{USER_CLIENT}+")      "]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	co.documentno Contract_number,
	coalesce(
		co.em_escm_notes,
		''
	) Contract_description,
	inv.documentno inv_num,
	to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							trunc(inv.dateinvoiced),
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			)
	inv_date,
	inv.Description,
	(
		case
			when sign( invl.LineNetAmt )= 1 then invl.LineNetAmt
			else 0
		end	
	) Dr_amt,
	abs( case when sign( invl.LineNetAmt )=- 1 then invl.LineNetAmt else 0 end ) Cr_amt,
	co.GrandTotal Cont_amt,
	coalesce(pbp.name ||' - '|| sbp.name ,bp.name) bp_name,
	e.value full_account,
	efin_getaccountingsequence(
		inv.DocumentNo,
		inv.AD_Client_ID,
		inv.AD_Org_ID
	) Yowmiya_num,
	(
		select
			rec.documentno
		from
			FIN_Reconciliation rec,
			EFIN_Fin_Reconciliationline recl,
			fin_payment pay
		where
			rec.fin_reconciliation_id = recl.fin_reconciliation_id
			and recl.fin_payment_id = pay.fin_payment_id --	and rec.documentno = '390000008'
			and pay.em_efin_invoice_id = inv.c_invoice_id limit 1
	) Taswiya_num,
	(select pay.EM_Efin_Paymentsequence from fin_payment pay where pay.em_efin_invoice_id = inv.c_invoice_id and pay.amount=abs(invl.LineNetAmt) limit 1) payment_seq_num,
	to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							now(),
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) as Hijri_date,
	To_char(
				now(),
				'DD/MM/YYYY'
			) Gregorian_date,
replace( $P{Inv_Date_From} ,'-','/') Inv_Date_From,
replace( $P{Inv_Date_To} ,'-','/') Inv_Date_To			
from
	c_elementvalue e,
	c_validcombination c,
--	escm_distribution_v dist,
	c_order co,
--	(select max( r.EM_Escm_Revision ) EM_Escm_Revision,r.documentno from c_order r group by r.documentno ) co2,
	c_bpartner bp,
	c_invoice inv,
	c_invoiceline invl
	left join c_bpartner pbp on invl.c_bpartner_id = pbp.c_bpartner_id
	left join c_bpartner sbp on invl.EM_Efin_Beneficiary2_ID = sbp.c_bpartner_id
where
	c.account_id = e.c_elementvalue_id
	and inv.c_bpartner_id=bp.c_bpartner_id
--	and co.documentno = co2.documentno 
--	and co.EM_Escm_Revision = co2.EM_Escm_Revision
    and CO.c_order_id=coalesce(inv.c_order_id,inv.EM_Efin_C_Order_ID)
	and inv.c_invoice_id = invl.c_invoice_id
--	and c.c_validcombination_id = dist.c_validcombination_id
--	and co.c_order_id = dist.c_order_id
    and c.c_validcombination_id = invl.em_efin_c_validcombination_id
	and e.accounttype = 'E'
	and inv.docstatus in ( 'CO','EFIN_WFA')
	and  (e.value not like '5%' and   e.value not like '9%' )
	and case when $P{Efin_C_Order_Report_V_ID} is not null then
	 co.documentno =(select documentno from c_order o where o.c_order_id = $P{Efin_C_Order_Report_V_ID})
	 else 1=1 end
	 $P!{AuxUSER_CLIENT} 
	 $P!{AUXinpDate} 	
order by
	e.value,
	co.documentno,
	inv.documentno,
	(select pay.EM_Efin_Paymentsequence from fin_payment pay where pay.em_efin_invoice_id = inv.c_invoice_id and pay.amount=abs(invl.LineNetAmt))]]>
	</queryString>
