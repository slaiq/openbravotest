	<parameter name="AUXinpDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{Inv_Date_From}==null || $P{Inv_Date_From}.equals( "" )) ?  "  and 1=1   " : "    and trunc(pay.Paymentdate)   BETWEEN trunc(eut_convertto_gregorian( '"+$P{Inv_Date_From}+"'  )) and trunc(eut_convertto_gregorian( '"+$P{Inv_Date_To}+"'  ))     "]]></defaultValueExpression>
	</parameter>
	<parameter name="AUX_inpAcct" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{element_from}==null || $P{element_from}.equals( "" )) ?  "    AND 1=1    " : "     AND e.VALUE BETWEEN (SELECT value  FROM c_elementvalue WHERE c_elementvalue_id = '"+$P{element_from}+"'      limit 1)   AND  (SELECT value  FROM c_elementvalue  WHERE c_elementvalue_id =  '"+$P{element_to}+"'   limit 1)         "]]></defaultValueExpression>
	</parameter>
	<parameter name="Payment_Status" class="java.lang.String"/>
	<parameter name="AuxPmtStatus" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{Payment_Status}==null || $P{Payment_Status}.equals( "" )) ?  "    AND 1=1    " : "   and  pay.status  in ('"+$P{Payment_Status}+"')    "]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select
	account,
	accout_name,
	payment_num,
	paymentdate,
	sum( amount ) amount,
	inv_num,
	inv_description,
	Cheque_num,
	Cheque_Date,
	coalesce(
		BP_name_Payment,
		BP_name_PayTo
	) bp_name,
	yowmiya_num,
	Reference_num,
	arabicdate,
	gregoriandate
from
	(
		select
			e.value account,
			e.name accout_name,
			pay.EM_Efin_Paymentsequence payment_num,
			to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							pay.Paymentdate,
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) Paymentdate,
			--		pay.Amount,
 invl.linenetamt Amount,
			inv.documentno inv_num,
			inv.description inv_Description,
			pay.em_efin_mofchequeno Cheque_num,
			to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							pay.em_efin_mofchequedate,
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) Cheque_Date,
			bp.value || '   ' || bp.name BP_name_PayTo,
			(
				select
					bp1.value || '   ' || bp1.name
				from
					C_Bpartner bp1
				where
					bp1.C_Bpartner_id = pay.EM_Efin_Bpartner_ID
			) BP_name_Payment,
		--	inv.Em_Efin_Accseq Yowmiya_num,
			efin_getaccountingsequence(
										inv.DocumentNo,
										inv.AD_Client_ID,
										inv.AD_Org_ID
										)  Yowmiya_num,
			(
				select
					EM_Escm_Reference_No
				from
					c_order
				where
					c_order_id = inv.c_order_id --and EM_Efin_C_Validcombination_ID = invl.EM_Efin_C_Validcombination_ID
			) Reference_num,
			to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							now(),
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) as ArabicDate,
			To_char(
				now(),
				'DD/MM/YYYY'
			) as GregorianDate
		from
			c_elementvalue e,
			fin_payment pay,
			c_invoice inv,
			c_invoiceline invl,
			C_Bpartner bp
		where
			pay.C_Bpartner_id = bp.C_Bpartner_id
			and inv.c_invoice_id = invl.c_invoice_id
			and e.c_elementvalue_id = invl.EM_Efin_C_Elementvalue_ID
			and pay.em_efin_invoice_id = inv.c_invoice_id
			--and invl.C_Bpartner_ID = pay.EM_Efin_Bpartner_ID 
			-- and   inv.c_bpartner_id <> pay.C_Bpartner_ID
		--	and pay.status = 'PPM'
			and e.accounttype = 'E'
			and e.elementlevel = 'S' 
			and invl.linenetamt >= 0 
			 and  invl.C_Bpartner_ID is null 
      and pay.EM_Efin_Bpartner_ID is null       
 		 $P!{AUXinpDate} 
       $P!{AUX_inpAcct}  
          $P!{AuxPmtStatus} 
UNION
		select
			e.value account,
			e.name accout_name,
			pay.EM_Efin_Paymentsequence payment_num,
			to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							pay.Paymentdate,
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) Paymentdate,
			--		pay.Amount,
 invl.linenetamt Amount,
			inv.documentno inv_num,
			inv.description inv_Description,
			pay.em_efin_mofchequeno Cheque_num,
			to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							pay.em_efin_mofchequedate,
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) Cheque_Date,
			bp.value || '   ' || bp.name BP_name_PayTo,
			(
				select
					bp1.value || '   ' || bp1.name
				from
					C_Bpartner bp1
				where
					bp1.C_Bpartner_id = pay.EM_Efin_Bpartner_ID
			) BP_name_Payment,
		--	inv.Em_Efin_Accseq Yowmiya_num,
		   efin_getaccountingsequence(
										inv.DocumentNo,
										inv.AD_Client_ID,
										inv.AD_Org_ID
										)  Yowmiya_num,
			(
				select
					EM_Escm_Reference_No
				from
					c_order
				where
					c_order_id = inv.c_order_id --and EM_Efin_C_Validcombination_ID = invl.EM_Efin_C_Validcombination_ID
			) Reference_num,
			to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							now(),
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) as ArabicDate,
			To_char(
				now(),
				'DD/MM/YYYY'
			) as GregorianDate
		from
			c_elementvalue e,
			fin_payment pay,
			c_invoice inv,
			c_invoiceline invl,
			C_Bpartner bp
		where
			pay.C_Bpartner_id = bp.C_Bpartner_id
			and inv.c_invoice_id = invl.c_invoice_id
			and e.c_elementvalue_id = invl.EM_Efin_C_Elementvalue_ID
			and pay.em_efin_invoice_id = inv.c_invoice_id
			and invl.C_Bpartner_ID = pay.EM_Efin_Bpartner_ID 
			-- and   inv.c_bpartner_id <> pay.C_Bpartner_ID
		--	and pay.status = 'PPM'
			and e.accounttype = 'E'
			and e.elementlevel = 'S' 
			and invl.linenetamt >= 0 
			-- and  invl.C_Bpartner_ID is null 
    --  and pay.EM_Efin_Bpartner_ID is null
 		 $P!{AUXinpDate} 
       $P!{AUX_inpAcct} 	   
        $P!{AuxPmtStatus} 
	) main
group by
	account,
	accout_name,
	payment_num,
	paymentdate,
	inv_num,
	inv_description,
	Cheque_num,
	Cheque_Date,
	bp_name,
	yowmiya_num,
	Reference_num,
	arabicdate,
	gregoriandate
order by
	main.account,
	main.payment_num]]>
	</queryString>
