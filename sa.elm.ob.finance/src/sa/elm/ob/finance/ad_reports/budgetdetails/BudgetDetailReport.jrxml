	<queryString>
		<![CDATA[select b.budgetname as Budget,(select em_efin_projacct from c_elementvalue where c_elementvalue_id = b.c_elementvalue_id) as flagfrprojact,(select name from c_elementvalue where c_elementvalue_id = b.c_elementvalue_id)
as budgetGroup,l.c_campaign_id as BudgetType,b.c_elementvalue_id,coalesce(sum(inq.funds_available),0) as FundsAvailable,sum(inq.encumbrance) as Encumbrance,e.name,
(select eut_convert_to_hijri(to_char(min(startdate),'YYYY-MM-DD')) from c_period where c_period_id = init.fromperiod) as startdate,
(select eut_convert_to_hijri(to_char(max(enddate),'YYYY-MM-DD')) from c_period where c_period_id = init.toperiod) as enddate,
(select name from ad_user usr where ad_user_id=$P{inpADUser}) as username,
sum(inq.current_budget) as CurrentBudget,sum(inq.REV_Amount) as OriginalAmt,l.description,e.name as acctname,e.value as acctsearchkey,p.name as projname,p.value as projsearchkey,sum(inq.Revinc_Amt) as RevisionIncrease,
sum(inq.Revdec_Amt) as RevisionDecrease,sum(inq.Spent_Amt) as Actual , eut_convert_to_hijri(to_char(now(),'YYYY-MM-DD')) as date,now() as time,concat(e.value,' - ',e.name) as account
from    efin_budgetint    init
join efin_budget b on b.efin_budgetint_id=init.efin_budgetint_id
join   efin_budgetlines l on l.efin_budget_id= b.efin_budget_id
 join c_project p on p.c_project_id = l.c_project_id
join c_elementvalue e on l.c_elementvalue_id = e.c_elementvalue_id
left join efin_budgetinquiry inq on b.efin_budgetint_id = inq.efin_budgetint_id and l.c_validcombination_id = inq.c_validcombination_id
join c_validcombination com on inq.c_validcombination_id = com.c_validcombination_id

where b.c_elementvalue_id=$P{inpBudgetGroupId1} and l.c_campaign_id = $P{inpBudgetTypeId1}
and init.efin_budgetint_id = $P{inpYear1} and b.status='APP' and l.c_salesregion_id in (select hq_budgetcontrolunit from efin_budget_ctrl_param bcp where bcp.isready='Y' and bcp.ad_client_id=b.ad_client_id)
and case when  $P{inpOrganizationId}='0' then 1=1 else com.ad_org_id=$P{inpOrganizationId} end

AND l.c_elementvalue_id IN (select act.c_elementvalue_id from efin_security_rules_act act
                    join efin_security_rules ru on ru.efin_security_rules_id=act.efin_security_rules_id
                    where ru.efin_security_rules_id=(select em_efin_security_rules_id from ad_role where ad_role_id= $P{inpRole})and efin_processbutton='Y')
          AND l.C_Salesregion_ID in (select dep.C_Salesregion_ID from Efin_Security_Rules_Dept dep
                    join efin_security_rules ru on ru.efin_security_rules_id=dep.efin_security_rules_id
                    where ru.efin_security_rules_id=(select em_efin_security_rules_id from ad_role where ad_role_id=$P{inpRole} )and efin_processbutton='Y')
          AND l.C_Project_ID in (select proj.c_project_id from efin_security_rules_proj proj
                    join efin_security_rules ru on ru.efin_security_rules_id=proj.efin_security_rules_id
                    where ru.efin_security_rules_id=(select em_efin_security_rules_id from ad_role where ad_role_id=$P{inpRole} )and efin_processbutton='Y')
          AND l.C_CAMPAIGN_ID IN(select bud.C_campaign_ID from efin_security_rules_budtype bud
                    join efin_security_rules ru on ru.efin_security_rules_id=bud.efin_security_rules_id
                    where ru.efin_security_rules_id=(select em_efin_security_rules_id from ad_role where ad_role_id=$P{inpRole} )and efin_processbutton='Y')
          AND l.C_Activity_ID in (select act.C_Activity_ID from efin_security_rules_activ act
                    join efin_security_rules ru on ru.efin_security_rules_id=act.efin_security_rules_id
                    where ru.efin_security_rules_id=(select em_efin_security_rules_id from ad_role where ad_role_id=$P{inpRole} )and efin_processbutton='Y')
          AND l.User1_ID in (select fut1.User1_ID from efin_security_rules_fut1 fut1
                    join efin_security_rules ru on ru.efin_security_rules_id=fut1.efin_security_rules_id
                    where ru.efin_security_rules_id=(select em_efin_security_rules_id from ad_role where ad_role_id=$P{inpRole} )and efin_processbutton='Y')
          AND l.User2_ID in (select fut2.User2_ID from efin_security_rules_fut2 fut2
                    join efin_security_rules ru on ru.efin_security_rules_id=fut2.efin_security_rules_id
                    where ru.efin_security_rules_id=(select em_efin_security_rules_id from ad_role where ad_role_id=$P{inpRole} )and efin_processbutton='Y')
$P!{AUX_inpOrg1} $P!{inpHideZeroValue}
 group by l.c_elementvalue_id,b.budgetname,flagfrprojact,budgetGroup,BudgetType,init.fromperiod,init.toperiod,
b.c_elementvalue_id,e.name ,l.description,e.value,p.name,p.value]]>
