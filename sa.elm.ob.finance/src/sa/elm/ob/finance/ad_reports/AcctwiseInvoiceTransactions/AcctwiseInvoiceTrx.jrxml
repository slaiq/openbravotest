		<defaultValueExpression><![CDATA[($P{C_Period_ID_Fr}==null || $P{C_Period_ID_Fr}.equals( "" )) ?  "    AND 1=1    " : " and per.periodno >= (select p.periodno from c_period p where p.c_period_id ='"+$P{C_Period_ID_Fr}+"' )    "]]></defaultValueExpression>
	</parameter>
