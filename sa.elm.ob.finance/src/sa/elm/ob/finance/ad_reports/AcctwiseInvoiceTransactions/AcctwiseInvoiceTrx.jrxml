	</parameter>
	<queryString>
		<![CDATA[select account,
name,
trans_type,
documentno inv_gl_num,
EM_Efin_Acctseq acct_sequence,
trunc(Cr,2) Cr,
trunc(Dr,2) Dr,
bpname,
description,
(
		select
		 to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					min( startdate ),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) 
		from
			c_period p
		where
			p.c_period_id = $P{C_Period_ID_Fr}  
	) period_start_date,
(
		select
		 to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					min( enddate ),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	)
		from
			c_period p
		where
			p.c_period_id = $P{C_Period_ID_To} 
	) period_end_date,	
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) as ArabicDate,
	To_char(
		now(),
		'DD/MM/YYYY'
	) as GregorianDate,
Case when   $P{Account_Type_Id} ='E' and  $P{AcctStatus_Id} ='Y' and $P{ChapterFrom_Id} is  null and $P{ChapterTo_Id} is  null then  'المنصرف على البند من واقع اليومية'	
     when   $P{Account_Type_Id} ='E' and  $P{AcctStatus_Id} ='N' and $P{ChapterFrom_Id} is  null and $P{ChapterTo_Id} is  null then  'المنصرف على البند' 
     when   $P{Account_Type_Id} ='E' and  $P{AcctStatus_Id} ='Y' and $P{ChapterFrom_Id} is not null and $P{ChapterTo_Id} is not null  then 'المنصرف على الباب من واقع اليومية'	 
	 when   $P{Account_Type_Id} ='E' and  $P{AcctStatus_Id} ='N' and $P{ChapterFrom_Id} is not null and $P{ChapterTo_Id} is not null  then 'لمنصرف على الباب'  
	 when   $P{Account_Type_Id} <>'E' and  $P{AcctStatus_Id} ='Y'  then 'المنصرف على الحساب من واقع اليومية' 
	 when   $P{Account_Type_Id} <>'E' and  $P{AcctStatus_Id} ='N'  then 'حركات الحساب'  end report_title
from 
(
select   e.value account,
	e.name,
	inv.documentno ,
	fa.EM_Efin_Acctseq,
	--sum(fa.AmtAcctCr) Cr,
	--sum(fa.AmtAcctDr) Dr,
/*	sum(fa.AmtAcctCr *
	(case
								when fa.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = fa.c_currency_id
									),
									1
								)
							end)) */ sum(fa.AmtAcctCr) Cr,
/*	sum(fa.AmtAcctDr *
	(case
								when fa.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = fa.c_currency_id
									),
									1
								)
							end))*/ sum(fa.AmtAcctDr) Dr,						
	--regexp_replace(inv.description,'[^\w]+','','g') description,
	trim(regexp_replace(inv.description,'\r|\n',' ','g')) description,
	bp.name bpname,
	'AP Posted' trans_type
from
	c_invoice inv,
	C_BPartner bp,
	Fact_Acct fa,
	Efin_Budgetint bint,
	c_elementvalue e,
	c_year yr,
	c_period per
where
	inv.documentno = fa.em_efin_documentno
	and inv.C_BPartner_ID=bp.C_BPartner_ID
	and fa.account_id = e.c_elementvalue_id
	and inv.EM_Efin_Budgetint_ID = bint.efin_budgetint_id
	and bint.c_year_id = yr.c_year_id
	and fa.c_period_id = per.c_period_id
	and bint.c_year_id = per.c_year_id
	and inv.docstatus = 'CO'
	and inv.posted = 'Y'
	and $P{AcctStatus_Id} ='Y'    
	and fa.EM_Efin_Acctseq is not null
    and  (e.value not like '5%' and   e.value not like '9%' )
    and case when  $P{Account_Type_Id} ='E' then e.accounttype ='E' else e.accounttype <>'E' end 
	and e.value between  	(	
	select min(value) from c_elementvalue e where 1=1
		and (case
			when $P{ChapterFrom_Id}  = '1' and  $P{Account_Type_Id} ='E' then e.value like '21%'
			when $P{ChapterFrom_Id}  = '2'  and  $P{Account_Type_Id} ='E' then(
				 e.value like '22%'
				or  e.value like '23%'
				or  e.value like '24%'
				or e.value like  '25%'
				or  e.value like '26%'
				or  e.value like '27%'
				or  e.value like '28%'
			)
			when $P{ChapterFrom_Id}  = '3' and  $P{Account_Type_Id} ='E' then e.value like  '3%'
			when $P{ChapterFrom_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e.value like '4%'
			else e.value like '%'
		end 		
		))
	and  (	
	select max(value) from c_elementvalue e where 1=1
		and (case
			when $P{ChapterTo_Id}  = '1' and  $P{Account_Type_Id} ='E' then e.value like '21%'
			when $P{ChapterTo_Id}  = '2' and  $P{Account_Type_Id} ='E' then(
				 e.value like '22%'
				or  e.value like '23%'
				or  e.value like '24%'
				or e.value like  '25%'
				or  e.value like '26%'
				or  e.value like '27%'
				or  e.value like '28%'
			)
			when $P{ChapterTo_Id}  = '3' and  $P{Account_Type_Id} ='E' then e.value like  '3%'
			when $P{ChapterTo_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e.value like '4%'
			else e.value like '%'
		end 		
		))
	and yr.c_year_id =  $P{C_Year_ID} 
     $P!{Aux_Period_from} 
 	 $P!{Aux_Period_to} 
	group by  e.value ,e.name,
			inv.documentno,
			fa.EM_Efin_Acctseq,
			inv.description,
			bp.name 
union all   
select 
	e.value account,
	e.name,
	inv.documentno,
	'' EM_Efin_Acctseq,
	case when sum(invl.LineNetAmt) <0 then abs(sum(invl.LineNetAmt*(case
								when inv.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = inv.c_currency_id
									),
									1
								)
							end))) else 0 end Cr,
	case when sum(invl.LineNetAmt) >0 then sum(invl.LineNetAmt*(case
								when inv.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = inv.c_currency_id
									),
									1
								)
							end)) else 0 end Dr,
	trim( regexp_replace( inv.description, '\r|\n', ' ', 'g' )) description,
	bp.name bpname,
	'AP Not Posted' trans_type
from
	c_invoice inv,
	c_invoiceline invl,
	c_elementvalue e,
	C_BPartner bp,
	Efin_Budgetint bint,
	c_year yr
where
	inv.c_invoice_id = invl.c_invoice_id
	and e.c_elementvalue_id = invl.EM_Efin_C_Elementvalue_ID
	and inv.C_BPartner_ID = bp.C_BPartner_ID
	and inv.em_efin_budgetint_id = bint.efin_budgetint_id
	and bint.c_year_id = yr.c_year_id
	and inv.docstatus = 'CO'
	and inv.posted = 'N'
	and $P{AcctStatus_Id} ='N'    
	and yr.c_year_id =  $P{C_Year_ID} 
	and  (e.value not like '5%' and   e.value not like '9%' )
    and case when  $P{Account_Type_Id} ='E' then e.accounttype ='E' else e.accounttype <>'E' end 
	and e.value between  	(	
	select min(value) from c_elementvalue e where 1=1
		and (case
			when $P{ChapterFrom_Id}  = '1' and  $P{Account_Type_Id} ='E' then e.value like '21%'
			when $P{ChapterFrom_Id}  = '2'  and  $P{Account_Type_Id} ='E' then(
				 e.value like '22%'
				or  e.value like '23%'
				or  e.value like '24%'
				or e.value like  '25%'
				or  e.value like '26%'
				or  e.value like '27%'
				or  e.value like '28%'
			)
			when $P{ChapterFrom_Id}  = '3' and  $P{Account_Type_Id} ='E' then e.value like  '3%'
			when $P{ChapterFrom_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e.value like '4%'
			else e.value like '%'
		end 		
		))
	and  (	
	select max(value) from c_elementvalue e where 1=1
		and (case
			when $P{ChapterTo_Id}  = '1' and  $P{Account_Type_Id} ='E' then e.value like '21%'
			when $P{ChapterTo_Id}  = '2' and  $P{Account_Type_Id} ='E' then(
				 e.value like '22%'
				or  e.value like '23%'
				or  e.value like '24%'
				or e.value like  '25%'
				or  e.value like '26%'
				or  e.value like '27%'
				or  e.value like '28%'
			)
			when $P{ChapterTo_Id}  = '3' and  $P{Account_Type_Id} ='E' then e.value like  '3%'
			when $P{ChapterTo_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e.value like '4%'
			else e.value like '%'
		end 		
		))
group by 
	e.value ,
	e.name,
	inv.documentno,
	inv.description,
	bp.name 			
 union all
select       e.value account,
             e.name,
			gl.documentno,
			fa.EM_Efin_Acctseq,
		--	sum( fa.AmtAcctCr ) Cr,
		--	sum( fa.AmtAcctDr ) Dr,
	/*	sum(fa.AmtAcctCr *
	(case
								when fa.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = fa.c_currency_id
									),
									1
								)
							end))*/  sum(fa.AmtAcctCr) Cr,
/*	sum(fa.AmtAcctDr *
	(case
								when fa.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = fa.c_currency_id
									),
									1
								)
							end)) */
            sum(fa.AmtAcctDr)	Dr,			
		--	regexp_replace(	gl.description,	'[^\w]+','','g'	) description,
		    trim(regexp_replace(gl.description,'\r|\n',' ','g')) description,
			bp.name bpname,
			'GL Posted' trans_type
		from
			Fact_Acct fa,
			Efin_Budgetint bint,
			c_elementvalue e,
			c_year yr,
			c_period per,
			GL_Journal gl
		left join C_BPartner bp on
			gl.C_BPartner_ID = bp.C_BPartner_ID
		where
			gl.documentno = fa.em_efin_documentno
			and fa.account_id = e.c_elementvalue_id
			and gl.em_efin_budgetint_id = bint.efin_budgetint_id
			and bint.c_year_id = yr.c_year_id
			and fa.c_period_id = per.c_period_id
			and bint.c_year_id = per.c_year_id
	--		and gl.EM_Efin_Adj_Invoice = 'N'
		--	and gl.docstatus = 'CO'
		--	and gl.posted = 'Y'
		      and case when  $P{AcctStatus_Id} ='Y' then  gl.posted = 'Y' else  gl.docstatus = 'CO'  end
			and fa.EM_Efin_Acctseq is not null 
			 and  (e.value not like '5%' and   e.value not like '9%' )
		--	and e.accounttype <>'E'
		    and case when  $P{Account_Type_Id} ='E' then e.accounttype ='E' else e.accounttype <>'E' end 
			and 
			e.value between  	(	
	select min(e1.value) from c_elementvalue e1 where 1=1
		and (case
			when $P{ChapterFrom_Id}  = '1' and  $P{Account_Type_Id} ='E' then e1.value like '21%'
			when $P{ChapterFrom_Id}  = '2' and  $P{Account_Type_Id} ='E' then(
				 e1.value like '22%'
				or  e1.value like '23%'
				or  e1.value like '24%'
				or e1.value like  '25%'
				or  e1.value like '26%'
				or  e1.value like '27%'
				or  e1.value like '28%'
			)
			when $P{ChapterFrom_Id}  = '3' and  $P{Account_Type_Id} ='E' then e1.value like  '3%'
			when $P{ChapterFrom_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e1.value like '4%'
			else e1.value like '%'
		end 		
		))
	and  (	
	select max(e1.value) from c_elementvalue e1 where 1=1
		and (case
			when $P{ChapterTo_Id}  = '1' and  $P{Account_Type_Id} ='E' then e1.value like '21%'
			when $P{ChapterTo_Id}  = '2' and  $P{Account_Type_Id} ='E' then(
				 e1.value like '22%'
				or  e1.value like '23%'
				or  e1.value like '24%'
				or e1.value like  '25%'
				or  e1.value like '26%'
				or  e1.value like '27%'
				or  e1.value like '28%'
			)
			when $P{ChapterTo_Id}  = '3' and  $P{Account_Type_Id} ='E' then e1.value like  '3%'
			when $P{ChapterTo_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e1.value like '4%'
			else e1.value like '%'
		end 		
		))
	and yr.c_year_id =  $P{C_Year_ID} 
     $P!{Aux_Period_from} 
 	 $P!{Aux_Period_to} 
	 group by   e.value ,e.name,
			gl.documentno,
			fa.EM_Efin_Acctseq,
			gl.description,
			bp.name
Union all
	select  e.value account,
            e.name,
			gl.documentno,
			'' EM_Efin_Acctseq,
		--	sum(gll.amtacctcr) Cr,
		--	sum(gll.amtacctdr) Dr,
		    sum(gll.AmtAcctCr *
	(case
								when gll.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = gll.c_currency_id
									),
									1
								)
							end)) Cr,
	sum(gll.AmtAcctDr *
	(case
								when gll.c_currency_id = '317' then 1
								else coalesce(
									(
										select
											multiplyrate
										from
											c_conversion_rate
										where
											c_currency_id = gll.c_currency_id
									),
									1
								)
							end)) Dr,	
		    trim(regexp_replace(gl.description,'\r|\n',' ','g')) description,
			bp.name bpname,
			'GL not Posted' trans_type
		from
			Efin_Budgetint bint,
			c_elementvalue e,
			c_year yr,
			gl_journalline gll,
			GL_Journal gl			
		left join C_BPartner bp on
			gl.C_BPartner_ID = bp.C_BPartner_ID
		where
			gl.gl_journal_id=gll.gl_journal_id
			and gll.em_efin_account = e.c_elementvalue_id
			and gl.em_efin_budgetint_id = bint.efin_budgetint_id
			and bint.c_year_id = yr.c_year_id
			and gl.docstatus = 'CO'
			and gl.posted = 'N'
			and $P{AcctStatus_Id} ='N'
		    and case when  $P{Account_Type_Id} ='E' then e.accounttype ='E' else e.accounttype <>'E' end 
			and yr.c_year_id =  $P{C_Year_ID} 
			and 
			e.value between  	(	
	select min(e1.value) from c_elementvalue e1 where 1=1
		and (case
			when $P{ChapterFrom_Id}  = '1' and  $P{Account_Type_Id} ='E' then e1.value like '21%'
			when $P{ChapterFrom_Id}  = '2' and  $P{Account_Type_Id} ='E' then(
				 e1.value like '22%'
				or  e1.value like '23%'
				or  e1.value like '24%'
				or e1.value like  '25%'
				or  e1.value like '26%'
				or  e1.value like '27%'
				or  e1.value like '28%'
			)
			when $P{ChapterFrom_Id}  = '3' and  $P{Account_Type_Id} ='E' then e1.value like  '3%'
			when $P{ChapterFrom_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e1.value like '4%'
			else e1.value like '%'
		end 		
		))
	and  (	
	select max(e1.value) from c_elementvalue e1 where 1=1
		and (case
			when $P{ChapterTo_Id}  = '1' and  $P{Account_Type_Id} ='E' then e1.value like '21%'
			when $P{ChapterTo_Id}  = '2' and  $P{Account_Type_Id} ='E' then(
				 e1.value like '22%'
				or  e1.value like '23%'
				or  e1.value like '24%'
				or e1.value like  '25%'
				or  e1.value like '26%'
				or  e1.value like '27%'
				or  e1.value like '28%'
			)
			when $P{ChapterTo_Id}  = '3' and  $P{Account_Type_Id} ='E' then e1.value like  '3%'
			when $P{ChapterTo_Id}  = '4' and  $P{Account_Type_Id} ='E' then  e1.value like '4%'
			else e1.value like '%'
		end 		
		))
		 group by   e.value ,e.name,
			gl.documentno,
			gl.description,
			bp.name			
	 ) main
	where 1=1
	  $P!{AUXinpAcctID} 
order by account,inv_gl_num,trans_type]]>
	</queryString>
	<field name="report_title" class="java.lang.String"/>
	<field name="account" class="java.lang.String"/>
	<field name="name" class="java.lang.String"/>
	<field name="trans_type" class="java.lang.String"/>
