:(!$P{From_c_project}.equals( "" ) && !$P{From_c_project}.equals( "null" ))?" and chldacct.c_elementvalue_id in (select c_elementvalue_id from c_elementvalue where value>=(select value from c_elementvalue where c_elementvalue_id ='"+$P{From_c_project}+"'))"
:(!$P{to_c_project}.equals( "" ) && !$P{to_c_project}.equals( "null" )) ?" and chldacct.c_elementvalue_id in (select c_elementvalue_id from c_elementvalue where value<=(select value from c_elementvalue where c_elementvalue_id ='"+$P{to_c_project}+"'))":""]]></defaultValueExpression>
	</parameter>
