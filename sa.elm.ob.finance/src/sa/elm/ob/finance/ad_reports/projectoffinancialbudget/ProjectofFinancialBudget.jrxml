	<queryString>
		<![CDATA[select chldacct.value as chldacctno, chldacct.name as chldacctname,
subacct.value as subacctno, subacct.name as subacctname,
cast((budinq.rev_amount/1000) as  decimal(16,2)) as originalbudg,
cast((budinq.current_budget/1000) as  decimal(16,2)) as current_budget,
cast((budinq.revinc_Amt/1000) as  decimal(16,2)) as revincamt,
cast((budinq.revdec_Amt/1000) as  decimal(16,2)) as revdecamt,
cast((budinq.revinc_Amt/1000)-(budinq.revdec_Amt/1000) as  decimal(16,2)) as revamt,
budtyp.em_efin_budgettype, eut_convert_to_hijri(to_char(now(),'YYYY-MM-dd')) as curdate,
(select name from c_elementvalue where c_elementvalue_id=
(select case when bacct.c_elementvalue_id is null then par.parent_id else chldacct.c_elementvalue_id end as parent
from efin_budgettype_acct bacct
right join (select replace(unnest(string_to_array(eut_getparentacct(chldacct.c_elementvalue_id, ''),
                                                  ',')::character varying [] ) ,'''','') as parent_id) par
on par.parent_id=bacct.c_elementvalue_id
left join c_elementvalue elem on elem.c_elementvalue_id=par.parent_id limit 1)) as parentname,
(select value from c_elementvalue where c_elementvalue_id=
(select case when bacct.c_elementvalue_id is null then par.parent_id else chldacct.c_elementvalue_id end as parent
from efin_budgettype_acct bacct
right join (select replace(unnest(string_to_array(eut_getparentacct(chldacct.c_elementvalue_id, ''),
                                                  ',')::character varying [] ) ,'''','') as parent_id) par
on par.parent_id=bacct.c_elementvalue_id
left join c_elementvalue elem on elem.c_elementvalue_id=par.parent_id limit 1)) as parentno,
chldacct.c_elementvalue_id, budinq.efin_budgetinquiry_id, cast((invln.linenetamt/1000) as  decimal(16,2)) as linenetamt, bint.name as budyear,
(select value from c_elementvalue  where c_elementvalue_id = $P{C_ElementValue_ID}) as chapter

from c_elementvalue chldacct
left join c_project subacct on subacct.c_project_id=chldacct.em_efin_project_id
left join efin_budgetinquiry budinq on budinq.c_elementvalue_id=chldacct.c_elementvalue_id and budinq.c_project_id=subacct.c_project_id
left join (select sum(linenetamt) as linenetamt, em_efin_c_elementvalue_id, c_project_id, em_efin_c_campaign_id
           from c_invoiceline where em_efin_c_elementvalue_id is not null $P!{AuxUSER_CLIENT}
group by em_efin_c_elementvalue_id, c_project_id, em_efin_c_campaign_id) invln
on invln.em_efin_c_elementvalue_id=chldacct.c_elementvalue_id and invln.c_project_id=subacct.c_project_id
join efin_budget_ctrl_param bcuacct on bcuacct.hq_budgetcontrolunit=budinq.c_salesregion_id
left join c_campaign budtyp on budtyp.c_campaign_id=budinq.c_campaign_id or budtyp.c_campaign_id=invln.em_efin_c_campaign_id
left join efin_budgetint bint on bint.efin_budgetint_id=budinq.efin_budgetint_id
where chldacct.accounttype='E' and chldacct.elementlevel='S'
$P!{AUX_BINQClient}
and budinq.efin_budgetint_id=$P{Efin_BudgetInt_id}
and case when (select count(*)
from efin_budgetinquiry inq
left join c_campaign budtyp on budtyp.c_campaign_id=inq.c_campaign_id
where budtyp.em_efin_budgettype='F' and inq.efin_budgetint_id=$P{Efin_BudgetInt_id}
               and inq.c_elementvalue_id=budinq.c_elementvalue_id
group by budinq.c_elementvalue_id
having count(*)>0)>0 then  budtyp.em_efin_budgettype='F'
else budtyp.em_efin_budgettype='C' end
and chldacct.c_elementvalue_id in (SELECT unnest(string_to_array(REPLACE(childids, LEFT(childids,1),''), ',')) as ids
                                   from (select eut_getchildacct($P{C_ElementValue_ID}) as childids)  parent )
$P!{AUX_Account}
order by chldacct.value asc, parentno asc]]>
	</queryString>
