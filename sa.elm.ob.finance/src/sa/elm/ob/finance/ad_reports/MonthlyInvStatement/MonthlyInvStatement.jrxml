	<queryString>
		<![CDATA[select
	(
		select
			co.documentno
		from
			c_order co
		where
			co.em_efin_budget_manencum_id = mencum.efin_budget_manencum_id limit 1
	) Contract_num,
	(
		select
			bidno
		from
			escm_bidmgmt bid,
			c_order co
		where
			bid.escm_bidmgmt_id = co.EM_Escm_Bidmgmt_ID
			and co.em_efin_budget_manencum_id = mencum.efin_budget_manencum_id limit 1
	) Bid_no,
	(
		select
			em_efin_documentno
		from
			c_bpartner bp
		where
			bp.c_bpartner_id = mencum.c_bpartner_id
	) supplier_num,
	(
		select
			name
		from
			c_bpartner bp
		where
			bp.c_bpartner_id = mencum.c_bpartner_id
	) supplier_name,
/*	(
		coalesce(
			manlin.amount,
			0
		)+ coalesce(
			manlin.enc_increase,
			0
		)- coalesce(
			manlin.enc_decrease,
			0
		)+(
			select
				abs( coalesce( sum( Revamount ), 0 ))
			from
				Efin_Bud_Manencum_Rev
			where
				efin_budget_manencumlines_id = manlin.efin_budget_manencumlines_id
		)
	)  
	coalesce(
		manlin.amount,
		0
	)  */
--manlin.Revamount	encum_amount,
--(select sum(a.Revamount) from efin_budget_manencumlines  a where  a.efin_budget_manencum_id= mencum.efin_budget_manencum_id) encum_amount, 
-- (select sum(a.System_Updated_Amt) from efin_budget_manencumlines  a where  a.efin_budget_manencum_id= mencum.efin_budget_manencum_id) encum_amount, --Changed as per Majed on 03/12/2019
case when mencum.Encum_Method='A' and (select sum(a.System_Updated_Amt) from efin_budget_manencumlines  a where  a.efin_budget_manencum_id= mencum.efin_budget_manencum_id)=0  --added as per Al Amir on 09/06/2020
   --  then (select sum(amount) from Escm_encumbrance_history_v scm where scm.efin_budget_manencumlines_id = manlin.efin_budget_manencumlines_id)
      then (select (Revamount) from Efin_Bud_Manencum_Rev sysmod where sysmod.efin_budget_manencumlines_id = manlin.efin_budget_manencumlines_id order by sysmod.created asc limit 1)  --added as per Majed on 31/08/2020     
	 else (select sum(a.System_Updated_Amt) from efin_budget_manencumlines  a where  a.efin_budget_manencum_id= mencum.efin_budget_manencum_id) end encum_amount,	 
replace(mencum.description,chr(10),'') encum_description,
	yr.year,
	mencum.documentno encumbrance_num,
	substr(
		e.value,
		1,
		1
	) Chapter,
	coalesce(
		(
			select
				value
			from
				c_elementvalue e1,
				ad_treenode tree
			where
				e1.c_elementvalue_id = tree.parent_id
				and e1.value not in(
					'1',
					'2',
					'3',
					'4',
					'5'
				)
				and tree.node_id = e.c_elementvalue_id
		),
		e.value
	) Account,
	coalesce(
		(
			select
				name
			from
				c_elementvalue e1,
				ad_treenode tree
			where
				e1.c_elementvalue_id = tree.parent_id
				and e1.value not in(
					'1',
					'2',
					'3',
					'4',
					'5'
				)
				and tree.node_id = e.c_elementvalue_id
		),
		e.name
	) account_name,
	e.value sub_account,
	e.name sub_acct_name,
	--	proj.value sub_account,
 --	proj.name sub_acct_name,
(
		select
			coalesce(
				sum( budglines.current_budget ),
				0
			)
		from
			efin_budgetinquiry budglines,
			c_salesregion salereg,
			c_campaign compgn
		where
			budglines.c_salesregion_id = salereg.c_salesregion_id
			and budglines.c_campaign_id = compgn.c_campaign_id
			and salereg.value = '990'
			and compgn.value = '01'
			and budglines.c_elementvalue_id = e.c_elementvalue_id
			and budglines.efin_budgetint_id = bint.efin_budgetint_id
	) current_budget,
(
		select
			coalesce(
				sum( budglines.Funds_Available ),
				0
			)
		from
			efin_budgetinquiry budglines,
			c_salesregion salereg,
			c_campaign compgn
		where
			budglines.c_salesregion_id = salereg.c_salesregion_id
			and budglines.c_campaign_id = compgn.c_campaign_id
			and salereg.value = '990'
			and compgn.value = '01'
			and budglines.c_elementvalue_id = e.c_elementvalue_id
			and budglines.efin_budgetint_id = bint.efin_budgetint_id
	) Bud_Fund_Available,	
	/*(
		select
			coalesce(
				sum( Revamt ),
				0
			)
		from
			Efin_Encum_Details_V a
		where
			efin_budget_manencumlines_id = manlin.efin_budget_manencumlines_id
			and efin_budgetint_id = bint.efin_budgetint_id
	) Prev_encum_amt,*/
	(
		select
			coalesce(
				sum( Revamt ),
				0
			)
		from
			Efin_Encum_Details_V a,
			efin_budgetinquiry budglines,
			c_salesregion salereg,
			c_campaign compgn
		where
			budglines.c_validcombination_id = a.c_validcombination_id
			and budglines.c_salesregion_id = salereg.c_salesregion_id
			and budglines.c_campaign_id = compgn.c_campaign_id --and salereg.value = '990'
 --and compgn.value = '01'
			and budglines.c_elementvalue_id = e.c_elementvalue_id
			and budglines.efin_budgetint_id = bint.efin_budgetint_id
	) Prev_encum_amt,
/*	coalesce(
		manlin.amount,
		0
	)   encumbrance_amount,*/
case when mencum.Encum_Method='A' and (select sum(a.System_Updated_Amt) from efin_budget_manencumlines  a where  a.efin_budget_manencum_id= mencum.efin_budget_manencum_id)=0  --added as per Al Amir on 09/06/2020
    -- then (select sum(amount) from Escm_encumbrance_history_v scm where scm.efin_budget_manencumlines_id = manlin.efin_budget_manencumlines_id)
	  then (select (Revamount) from Efin_Bud_Manencum_Rev sysmod where sysmod.efin_budget_manencumlines_id = manlin.efin_budget_manencumlines_id order by sysmod.created asc limit 1)  --added as per Majed on 31/08/2020       
	 else (select sum(a.System_Updated_Amt) from efin_budget_manencumlines  a where  a.efin_budget_manencum_id= mencum.efin_budget_manencum_id
	       and a.c_elementvalue_id = e.c_elementvalue_id) end 	encumbrance_amount,
--	manlin.Revamount	 encumbrance_amount,
	(
		select
			coalesce(
				sum( Revamt ),
				0
			)
		from
			Efin_Encum_Details_V
		where
			efin_budget_manencum_id = mencum.efin_budget_manencum_id
			and docstatus = 'CO'
	) Completed_encum_amt,
	manlin.funds_available,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) as ArabicDate,
	To_char(
		now(),
		'DD/MM/YYYY'
	) as GregorianDate,
	(
	select
			c.name
		from
			ad_role a,
			ad_user_roles b,
			ad_user c
		where
			a.ad_role_id = b.ad_role_id
			and b.ad_user_id = c.ad_user_id
			and b.isactive = 'Y'
			and c.isactive = 'Y'
			and c.ad_user_id <> '100'
		--	and a.name = 'مدير عام الادارة المالية'
		     and a.ad_role_id ='2E78C8B91DC84BDABC02F1FA081BFDA6'	
	--select name from ad_user where username ='700053' limit 1		 
	) mudeer_aam,
	(
		select
			c.name
		from
			ad_role a,
			ad_user_roles b,
			ad_user c
		where
			a.ad_role_id = b.ad_role_id
			and b.ad_user_id = c.ad_user_id
			and b.isactive = 'Y'
			and c.isactive = 'Y'
			and c.ad_user_id <> '100'
		--	and a.name = 'مدير إدارة الميزانية'
		    and a.ad_role_id='FB6C84C9CCBF486E9C65F87143636BF3'
		    limit 1
	) mudeer_idara,
	(
		select
			name
		from
			ad_user
		where
			ad_user_id = mencum.updatedby
	) Created_by,
	
	 coalesce(
			manlin.enc_increase,
			0
		) enc_increase,
	coalesce(
			manlin.enc_decrease,
			0
		)	enc_decrease , 
	--case when manlin.enc_increase=0 then '0' else manlin.enc_increase end enc_increase1,
	--case when manlin.enc_decrease=0 then '0' else manlin.enc_decrease end enc_decrease1
	case when (select sum(enc_increase) from efin_budget_manencumlines a where a.efin_budget_manencum_id = mencum.efin_budget_manencum_id)=0 then 'N' else 'Y' end     enc_increase1,
	case when (select sum(enc_decrease) from efin_budget_manencumlines a where a.efin_budget_manencum_id = mencum.efin_budget_manencum_id)=0 then 'N' else 'Y' end     enc_decrease1
	,'SAR'  curcode
	,(select count(value) from c_elementvalue a,efin_budget_manencumlines b where a.c_elementvalue_id = b.c_elementvalue_id and b.efin_budget_manencum_id = mencum.efin_budget_manencum_id and value like'9%') ch9_cnt
from
	efin_budget_manencum mencum,
	efin_budget_manencumlines manlin,
	efin_budgetint bint,
	c_year yr,
	c_elementvalue e,
	--C_Project proj,
 c_campaign cam
where
	1 = 1
	and mencum.efin_budgetint_id = bint.efin_budgetint_id
	and mencum.efin_budget_manencum_id = manlin.efin_budget_manencum_id
	and manlin.c_elementvalue_id = e.c_elementvalue_id
	and bint.c_year_id = yr.c_year_id --and manlin.c_project_id = proj.c_project_id
	and mencum.c_campaign_id = cam.c_campaign_id
	and cam.value = '01'
	and e.accounttype = 'E' --	
	--and mencum.documentno = '400008752'
	and(
		e.value like '3%'
		or e.value like '4%'
	) 
	$P!{AUXinpYearId} 
   $P!{AUXinpEncumId}]]>
	</queryString>
