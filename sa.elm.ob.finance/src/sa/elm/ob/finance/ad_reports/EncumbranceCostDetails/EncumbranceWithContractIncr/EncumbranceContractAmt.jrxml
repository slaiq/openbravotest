	<parameter name="Efin_Budget_Manencum_Report_V_ID" class="java.lang.String"/>
	<parameter name="AUXinpYearId" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{C_Year_ID}==null || $P{C_Year_ID}.equals( "" )) ?  "    AND 1=1    " : "   and yr.c_year_id in ('"+$P{C_Year_ID}+"')    "]]></defaultValueExpression>
	</parameter>
	<parameter name="AUXinpEncumId" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{Efin_Budget_Manencum_Report_V_ID}==null || $P{Efin_Budget_Manencum_Report_V_ID}.equals( "" )) ?  "    AND 1=1    " : "   and mencum.Efin_Budget_Manencum_ID in ('"+$P{Efin_Budget_Manencum_Report_V_ID}+"')    "]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	co.documentno Contract_num,
	--co.description Contract_description,
	(select col.em_escm_prodescription from c_orderline col where col.c_order_id=co.c_order_id limit 1) Contract_description,
	(
		select
			em_efin_documentno
		from
			c_bpartner bp
		where
			bp.c_bpartner_id = mencum.c_bpartner_id
	) supplier_num,
	(
		select
			name
		from
			c_bpartner bp
		where
			bp.c_bpartner_id = mencum.c_bpartner_id
	) supplier_name,
	coalesce(
		manlin.amount,
		0
	) Contract_amount,
	coalesce(
			manlin.enc_increase,
			0
		) Increase_amount,
	trunc((coalesce(
			manlin.enc_increase,
			0
		)/coalesce(
			manlin.amount,
			0
		)  ),2)*100 inc_percent_amount,	
		(
		select
			coalesce(
				sum( budglines.current_budget ),
				0
			)
		from
			efin_budgetinquiry budglines,
			c_salesregion salereg,
			c_campaign compgn
		where
			budglines.c_salesregion_id = salereg.c_salesregion_id
			and budglines.c_campaign_id = compgn.c_campaign_id
			and salereg.value = '990'
			and compgn.value = '01'
			and budglines.c_elementvalue_id = e.c_elementvalue_id
			and budglines.efin_budgetint_id = bint.efin_budgetint_id
	) current_budget,
	manlin.funds_available,
	((
		select
			coalesce(
				sum( budglines.current_budget ),
				0
			)
		from
			efin_budgetinquiry budglines,
			c_salesregion salereg,
			c_campaign compgn
		where
			budglines.c_salesregion_id = salereg.c_salesregion_id
			and budglines.c_campaign_id = compgn.c_campaign_id
			and salereg.value = '990'
			and compgn.value = '01'
			and budglines.c_elementvalue_id = e.c_elementvalue_id
			and budglines.efin_budgetint_id = bint.efin_budgetint_id
	) -	coalesce(manlin.funds_available,0)) Remaining_amt,
	(
		select
			lkp.name
		from
			ESCM_DefLookups_TypeLn lkp
		where
			lkp.escm_deflookups_typeln_id = co.EM_Escm_Contact_Type
	) contract_Category,
		coalesce(
		(
			select
				value
			from
				c_elementvalue e1,
				ad_treenode tree
			where
				e1.c_elementvalue_id = tree.parent_id
				and e1.value not in(
					'1',
					'2',
					'3',
					'4',
					'5'
				)
				and tree.node_id = e.c_elementvalue_id
		),
		e.value
	) Account,
	coalesce(
		(
			select
				name
			from
				c_elementvalue e1,
				ad_treenode tree
			where
				e1.c_elementvalue_id = tree.parent_id
				and e1.value not in(
					'1',
					'2',
					'3',
					'4',
					'5'
				)
				and tree.node_id = e.c_elementvalue_id
		),
		e.value
	) Account_name,
	e.value sub_account,
	e.name sub_acct_name,
	yr.year  fin_year,
	(
	/*	select
			c.name
		from
			ad_role a,
			ad_user_roles b,
			ad_user c
		where
			a.ad_role_id = b.ad_role_id
			and b.ad_user_id = c.ad_user_id
			and b.isactive = 'Y'
			and c.isactive = 'Y'
			and c.ad_user_id <> '100'
			and a.name = 'مدير عام الادارة المالية'  */
		select name from ad_user where username ='700053' limit 1	 
	) mudeer_aam,
	(
		select
			c.name
		from
			ad_role a,
			ad_user_roles b,
			ad_user c
		where
			a.ad_role_id = b.ad_role_id
			and b.ad_user_id = c.ad_user_id
			and b.isactive = 'Y'
			and c.isactive = 'Y'
			and c.ad_user_id <> '100'
		--	and a.name = 'مدير إدارة الميزانية'
			and a.ad_role_id='FB6C84C9CCBF486E9C65F87143636BF3'
	) mudeer_idara,
	--(select name from ad_user where ad_user_id =mencum.createdby ) creator,
	'مهندس ادارة الصيانة' creator,
	(select name from ad_user where ad_user_id =mencum.updatedby ) updated_by,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) as ArabicDate,
	To_char(
		now(),
		'DD/MM/YYYY'
	) as GregorianDate
from
	efin_budget_manencumlines manlin,
	efin_budgetint bint,
	c_year yr,
	c_elementvalue e,
	c_campaign cam,
	efin_budget_manencum mencum
left join c_order co on
	mencum.efin_budget_manencum_id = co.em_efin_budget_manencum_id
where
	1 = 1
	and mencum.efin_budgetint_id = bint.efin_budgetint_id
	and mencum.efin_budget_manencum_id = manlin.efin_budget_manencum_id
	and manlin.c_elementvalue_id = e.c_elementvalue_id
	and bint.c_year_id = yr.c_year_id --and manlin.c_project_id = proj.c_project_id
	and mencum.c_campaign_id = cam.c_campaign_id
	and cam.value = '01'
	and e.accounttype = 'E' --	
--	and mencum.documentno = '400007470'
	and(
		e.value like '3%'
		or e.value like '4%'
	)
	$P!{AUXinpYearId} 
  $P!{AUXinpEncumId}]]>
	</queryString>
