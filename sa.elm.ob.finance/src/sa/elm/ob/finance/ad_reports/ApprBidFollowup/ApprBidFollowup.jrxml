	<parameter name="C_Year_ID" class="java.lang.String"/>
	<parameter name="escm_bidmgmt_id" class="java.lang.String"/>
	<parameter name="Aux_Year_id" class="java.lang.String" isForPrompting="false"/>
	<parameter name="Aux_Bid_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{escm_bidmgmt_id}==null || $P{escm_bidmgmt_id}.equals( "" )) ?  "    AND 1=1    " : "  and  bid.escm_bidmgmt_id = '"+$P{escm_bidmgmt_id}+"'     "]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	distinct 
	yr.year  fin_year,
	bid.bidname,
	bid.bidno,
	coalesce(
		prop.Totalamount,
		0
	) total_amt,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					bid.bidcreationdate,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) bid_creation_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					hist.approveddate,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) approval_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					bdate.Openenvday,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) Open_env_day,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					techeval.approveddate,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) tech_eval_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					prop_eval.approveddate,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) prop_eval_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					co.EM_Escm_Signaturedate,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) signature_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					po_hist.approveddate,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) po_hist_date,
	bp.name bp_name,
	--prop.totalamount total_amt,
/*	case
		when bid.bidappstatus = 'ESCM_AP' then 'Approved'
		when bid.bidappstatus = 'ESCM_RA' then 'Require Re-Approval'
		when bid.bidappstatus = 'DR' then 'Draft'
		when bid.bidappstatus = 'ESCM_REJ' then 'Rejected'
		when bid.bidappstatus = 'ESCM_IP' then 'In Progress'
	end bid_appr_Status,  */
	(
	select
		tr.name
	from
		AD_Ref_List_Trl tr,
		AD_Ref_List list
	where
		tr.ad_ref_list_id = list.ad_ref_list_id
		and list.value = bid.bidappstatus
		and list.ad_reference_id = 'CB918131BBF1426681029BE77C0A86C2'  
		limit 1
	)  bid_appr_Status,
	CO.EM_Escm_Receive_Mof_Hijiri receive_mof_date,
	co.EM_Escm_Mof_Date_Hijiri  send_mof_date,
	prop_eval.pee_appr_date_h ,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					prop_hist.prop_hist_date,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	)
	prop_hist_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	)as Hijri_date,
	To_char(
		now(),
		'DD/MM/YYYY'
	) as GregorianDate --	,bid.*
 --,bidl.em_efin_budgmanencumline_id
from c_year yr,
	escm_bidmgmt_line bidl,
	escm_bidmgmt bid
left join escm_proposalmgmt prop on
	bid.escm_bidmgmt_id = prop.escm_bidmgmt_id
	and prop.proposalstatus = 'AWD'
left join 
(select
	max( Approveddate ) prop_hist_date,
	max( seqno ),
	escm_proposalmgmt_id
from
	Escm_Proposalmgmt_Hist
group by escm_proposalmgmt_id) prop_hist on prop.escm_proposalmgmt_id	=prop_hist.escm_proposalmgmt_id	
left join c_bpartner bp on
	prop.Supplier = bp.c_bpartner_id
left join c_order co on
	prop.escm_proposalmgmt_id = co.em_escm_proposalmgmt_id
left join(
		select
			max( seqno ),
			max( Approveddate ) approveddate,
			c_order_id
		from
			escm_purorderacthist
		group by
			c_order_id
	) po_hist on
	co.c_order_id = po_hist.c_order_id
left join(
		select
			max( seqno ),
			max( approveddate ) approveddate,
			escm_bidmgmt_id
		from
			escm_bidmgmt_hist
		group by
			escm_bidmgmt_id
	) hist on
	bid.escm_bidmgmt_id = hist.escm_bidmgmt_id
left join escm_biddates bdate on
	bid.escm_bidmgmt_id = bdate.escm_bidmgmt_id
left join(
		select
			hist.approveddate,
			tech.escm_bidmgmt_id
		from
			escm_technicalevl_event tech,
			(
				select
					max( seqno ),
					max( approveddate ) approveddate,
					escm_technicalevl_event_id
				from
					ESCM_Techevlevent_Apphist
				group by
					escm_technicalevl_event_id
			) hist
		where
			tech.status = 'CO'
			and tech.escm_technicalevl_event_id = hist.escm_technicalevl_event_id
	) techeval on
	bid.escm_bidmgmt_id = techeval.escm_bidmgmt_id
left join(
		select
			hist.approveddate,
			prp_eval.escm_bidmgmt_id,
			prp_eval.Escm_Approval_Date_Hijiri pee_appr_date_h
		from
			ESCM_ProposalEvl_Event prp_eval,			
			escm_proposalmgmt prop2,
			escm_proposal_attr propls
		left join(
				select
					max( seqno ),
					max( approveddate ) approveddate,
					escm_proposal_attr_id
				from
					Escm_ProposalStatus_Hist
				group by
					escm_proposal_attr_id
			) hist on
			propls.escm_proposal_attr_id = hist.escm_proposal_attr_id
		where
			prp_eval.escm_proposalevl_event_id = propls.escm_proposalevl_event_id
			and prop2.escm_proposalmgmt_id = propls.escm_proposalmgmt_id
			and prop2.proposalstatus = 'AWD'
			and prp_eval.status = 'CO'
	) prop_eval on
	bid.escm_bidmgmt_id = prop_eval.escm_bidmgmt_id
where
	bid.escm_bidmgmt_id = bidl.escm_bidmgmt_id
	and bid.finanyear = yr.c_year_id
--	and bid.bidstatus = 'ACT'
--	and bid.bidappstatus = 'ESCM_AP' 
	and yr.c_year_id =  $P{C_Year_ID} 
   $P!{Aux_Bid_id} 
	--	and bid.bidno ='2020DP00250'
 --and bid.bidno='1440BD1000070'
 order by bid.bidno]]>
	</queryString>
