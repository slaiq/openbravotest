    <PARAMETER id="fieldOrg" name="orgId" attribute="value"/>
    <PARAMETER id="fieldPaymentProposal" name="paymentProposalId" attribute="value"/>
    <PARAMETER id="fieldWindowId" name="windowId" attribute="value"/>
    <PARAMETER id="fieldTabId" name="tabId" attribute="value"/>
    <PARAMETER id="fieldReceipt" name="isReceipt" attribute="value"/>
    <PARAMETER id="paramProposalNo" name="proposalNo"/>
    <PARAMETER id="paramPaymentMethod" name="paymentMethod"/>
    <PARAMETER id="paramBusinessPartner" name="businessPartner"/>
    <PARAMETER id="paramDueDate" name="dueDate"/>
