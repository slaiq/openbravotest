	<queryString>
		<![CDATA[select gl.documentno,eut_convert_to_hijri(to_char(gl.datedoc,'YYYY-MM-DD')) as dateofjournal, facct.em_efin_acctseq as acctseq,
--eut_convert_to_hijri(to_char(gl.dateacct,'YYYY-MM-DD')) as acctdate
eut_convert_to_hijri(to_char(facct.dateacct,'YYYY-MM-DD')) as acctdate
, prd.name as themonth, coalesce(yr.description, yr.year) as finyear,
p.name as project,gll.description,
round(gll.amtsourcecr,2) as creditamt,
(select sum(round(gll.amtsourcecr,2)) from gl_journalline gll
where gll.gl_journal_id  =$P{inpGlJournalId}  and gll.amtsourcecr>'0')  as sumcreditamt,
case when gll.amtsourcedr>0 then    (select sum(round(gll.amtsourcedr,2)) from gl_journalline gll join C_ValidCombination vc on
vc.C_ValidCombination_ID=gll.C_ValidCombination_ID join c_elementvalue e on e.c_elementvalue_id=vc.account_id
where gll.gl_journal_id  =$P{inpGlJournalId} ) else null end  as sumdebitamt,
usr.name as creator, sreg.name as dept
,(
	/*	select
			u1.name manager_name
		from
			ad_user u,
			ad_user u1,
			c_bpartner b
		where
			u.c_bpartner_id = b.c_bpartner_id
			and u1.username = b.EM_Ehcm_Manager
			and u.ad_user_id = gl.createdby  */
	select
	u1.name
from
	Efin_User_Manager mgr,
	ad_user u,
	ad_user u1
where
	mgr.ad_user_id = u.ad_user_id
	and mgr.line_manager_id = u1.ad_user_id
	and u.ad_user_id = gl.createdby
     limit 1	
	) User_manager,
(
		select
			c.name
		from
			ad_role a,
			ad_user_roles b,
			ad_user c
		where
			a.ad_role_id = b.ad_role_id
			and b.ad_user_id = c.ad_user_id
			and b.isactive = 'Y'
			and c.isactive = 'Y'
			and c.ad_user_id <> '100'
			and a.ad_role_id ='D8712712FBAD40F98C1D4BEBACA5008F'
			and c.ad_user_id='EB70337DB9F748BD86034107804F3C1A'
		--	and a.name = 'مدير عام ادارة المنافسات والعقود'
	) sahebe_salahiya,
(
select c.name from ad_role a, ad_user_roles b ,ad_user c
where a.ad_role_id=b.ad_role_id
and b.ad_user_id = c.ad_user_id
and b.isactive ='Y'
and c.isactive ='Y'
and c.ad_user_id <> '100'
and a.ad_role_id ='2E78C8B91DC84BDABC02F1FA081BFDA6'
--and c.ad_user_id='ABED27AB17CC47AA8CD4B49236B71836'
limit 1
--and a.name='مدير عام الادارة المالية'
) mudeer_idara,
(
select c.name from ad_role a, ad_user_roles b ,ad_user c
where a.ad_role_id=b.ad_role_id
and b.ad_user_id = c.ad_user_id
and b.isactive ='Y'
and c.isactive ='Y'
and c.ad_user_id <> '100'
and a.ad_role_id='55C03A9EAA134110BD5A5851D2002C2A'
--and a.name='الممثل المالي'
) Financial_Rep	,
to_char(
				to_Date(
					Eut_convert_to_hijri(
						To_char(
							now(),
							'YYYY-MM-DD'
						)
					)
				),
				'DD/MM/YYYY'
			) as ArabicDate,
			To_char(
				now(),
				'DD/MM/YYYY'
			) as GregorianDate
from gl_journal gl
join gl_journalline  gll on gll.gl_journal_id=gl.gl_journal_id
join C_ValidCombination vc on vc.C_ValidCombination_ID=gll.C_ValidCombination_ID
join c_project p on p.C_Project_ID =gll.C_Project_ID
left join fact_acct facct on facct.record_id=gl.gl_journal_id
left join c_period prd --on prd.c_period_id=gl.c_period_id
             on prd.c_period_id=facct.c_period_id
join Efin_Budgetint bint  on gl.em_efin_budgetint_id=bint.efin_budgetint_id			 
left join c_year yr --on yr.c_year_id=prd.c_year_id
           on yr.c_year_id=bint.c_year_id
left join ad_user usr on usr.ad_user_id=gl.createdby
left join c_bpartner bp on bp.c_bpartner_id=usr.c_bpartner_id
left join c_salesregion sreg on sreg.c_salesregion_id=bp.em_efin_salesregion
where gl.gl_journal_id  = $P{inpGlJournalId}
group by gll.amtsourcedr,gll.amtsourcecr,vc.account_id,p.name,gl.documentno,gl.dateacct,gll.description,
gl.datedoc, prd.name, yr.year, facct.em_efin_acctseq, usr.name, sreg.name, yr.description,gl.createdby,facct.dateacct
order by creditamt limit 1]]>
	</queryString>
