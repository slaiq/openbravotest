	<queryString>
		<![CDATA[(select gl.documentno,eut_convert_to_hijri(to_char(gl.datedoc,'YYYY-MM-DD')) as dateofjournal, facct.em_efin_acctseq as acctseq,
eut_convert_to_hijri(to_char(gl.dateacct,'YYYY-MM-DD')) as acctdate, pr.name as themonth, coalesce(yr.description, yr.year) as finyear,
p.name as project,gll.description, round(gll.amtsourcecr,2) as creditamt,
(select sum(round(gll.amtsourcecr,2)) from gl_journalline gll
where gll.gl_journal_id in( select gl_journal_id from gl_journal where documentno =$P{inpRequestNo}
and c_doctype_id =$P{inphDoctypelist} ) and gll.amtsourcecr>'0')  as sumcreditamt,
case when gll.amtsourcedr>0 then (select sum(round(gll.amtsourcedr,2))
from gl_journalline gll join C_ValidCombination vc on vc.C_ValidCombination_ID=gll.C_ValidCombination_ID
join c_elementvalue e on e.c_elementvalue_id=vc.account_id
where gll.gl_journal_id  =$P{inpGlJournalId}) else null end  as sumdebitamt,
usr.name as creator, sreg.name as dept, yr.C_Calendar_id
,(
		select
			u1.name manager_name
		from
			ad_user u,
			ad_user u1,
			c_bpartner b
		where
			u.c_bpartner_id = b.c_bpartner_id
			and u1.username = b.EM_Ehcm_Manager
			and u.ad_user_id = gl.createdby
	) User_manager,
(
		select
			c.name
		from
			ad_role a,
			ad_user_roles b,
			ad_user c
		where
			a.ad_role_id = b.ad_role_id
			and b.ad_user_id = c.ad_user_id
			and b.isactive = 'Y'
			and c.isactive = 'Y'
			and c.ad_user_id <> '100'
			and a.name = 'مدير عام ادارة المنافسات والعقود'
	) sahebe_salahiya,
(
select c.name from ad_role a, ad_user_roles b ,ad_user c
where a.ad_role_id=b.ad_role_id
and b.ad_user_id = c.ad_user_id
and b.isactive ='Y'
and c.isactive ='Y'
and c.ad_user_id <> '100'
and a.name='مدير عام الادارة المالية'
) mudeer_idara,
(
select c.name from ad_role a, ad_user_roles b ,ad_user c
where a.ad_role_id=b.ad_role_id
and b.ad_user_id = c.ad_user_id
and b.isactive ='Y'
and c.isactive ='Y'
and c.ad_user_id <> '100'
and a.name='الممثل المالي'
) Financial_Rep	
from gl_journal gl join gl_journalline  gll on gll.gl_journal_id=gl.gl_journal_id
join C_ValidCombination vc on vc.C_ValidCombination_ID=gll.C_ValidCombination_ID
join c_project p on p.C_Project_ID =gll.C_Project_ID
join c_period pr on pr.c_period_id=gl.c_period_id
join c_year yr on yr.c_year_id=pr.c_year_id
left join fact_acct facct on facct.record_id=gl.gl_journal_id
left join ad_user usr on usr.ad_user_id=gl.createdby
left join c_bpartner bp on bp.c_bpartner_id=usr.c_bpartner_id
left join c_salesregion sreg on sreg.c_salesregion_id=bp.em_efin_salesregion
where gl.gl_journal_id  =$P{inpGlJournalId} and gl.posted='Y' and gl.ad_client_id =$P{inpClientId}
group by gl.documentno, gl.datedoc, facct.em_efin_acctseq, gl.dateacct, pr.name, finyear,yr.C_Calendar_id,
p.name,gll.description, gll.amtsourcedr,gll.amtsourcecr,vc.account_id, usr.name, sreg.name,gl.createdby
order by creditamt limit 1)

union
(select inv.documentno, eut_convert_to_hijri(to_char(inv.dateinvoiced,'YYYY-MM-DD')) as dateofjournal, fc.em_efin_acctseq as acctseq,
eut_convert_to_hijri(to_char(inv.dateacct,'YYYY-MM-DD')) as acctdate, pr.name as themonth, coalesce(yr.Description,yr.Year) as finyear,
p.name as project, li.description, round(fc.amtsourcecr,2) as creditamt,
(select sum(round(fc.amtsourcecr,2)) from fact_acct fc
where fc.record_id  =$P{inpGlJournalId} and fc.amtsourcecr>'0')  as sumcreditamt,
case when fc.amtsourcedr>0 then (select sum(round(fc.amtsourcedr,2)) from fact_acct fc
where fc.record_id  =$P{inpGlJournalId} ) else null end  as sumdebitamt,
usr.name as creator, sreg.name as dept, cal.C_Calendar_id,
'' User_manager,
'' sahebe_salahiya,
'' mudeer_idara,
'' Financial_Rep
from c_invoice inv join fact_acct fc on inv.c_invoice_id =fc.record_id
join c_invoiceline li on li.c_invoice_id = inv.c_invoice_id
join c_elementvalue e on e.c_elementvalue_id = li.em_efin_c_elementvalue_id
join c_doctype dc on dc.c_doctype_id = inv.c_doctypetarget_id
join c_project p on p.C_Project_ID =fc.C_Project_ID
join AD_Org org on org.AD_Org_ID=inv.AD_Org_ID
join C_Calendar cal on cal.C_Calendar_id=org.C_Calendar_id
join c_year yr on yr.c_calendar_id=cal.c_calendar_id
join c_period pr on pr.c_year_id=yr.c_year_id
left join ad_user usr on usr.ad_user_id=inv.createdby
left join c_bpartner bp on bp.c_bpartner_id=usr.c_bpartner_id
left join c_salesregion sreg on sreg.c_salesregion_id=bp.em_efin_salesregion
where inv.c_invoice_id  =$P{inpGlJournalId} and inv.dateacct>=pr.StartDate and inv.dateacct<=pr.EndDate
and dc.em_efin_isprepayinvapp = 'Y' and inv.posted ='Y' and inv.documentno =$P{inpRequestNo}
and inv.ad_client_id =$P{inpClientId} and dc.c_doctype_id =$P{inphDoctypelist}
group by inv.documentno, inv.dateinvoiced, fc.em_efin_acctseq, inv.dateacct, pr.name, finyear, cal.C_Calendar_id,
p.name,inv.description, fc.amtsourcedr,fc.amtsourcecr, li.description, usr.name, sreg.name
order by creditamt limit 1)

union
(select recon.documentno, eut_convert_to_hijri(to_char(fc.datetrx,'YYYY-MM-DD')) as dateofjournal, fc.em_efin_acctseq as acctseq,
eut_convert_to_hijri(to_char(recon.Statementdate,'YYYY-MM-DD')) as Statementdate,
(select prd.name from c_period prd
left join c_year yr on yr.c_year_id=prd.c_year_id
where CAST(recon.Statementdate as date) BETWEEN startdate and enddate and yr.C_Calendar_id=cal.C_Calendar_id) as themonth,
(select coalesce(yr.Description,yr.Year) from c_period prd
left join c_year yr on yr.c_year_id=prd.c_year_id
where CAST(recon.Statementdate as date) BETWEEN startdate and enddate and yr.C_Calendar_id=cal.C_Calendar_id) as Descriptionyear,
p.name as project, recon.EM_Efin_Description as description,
round(fc.amtsourcecr,2) as creditamt,
(select sum(round(fc.amtsourcecr,2)) from fact_acct fc
where fc.record_id  =$P{inpGlJournalId} and fc.amtsourcecr>'0')  as sumcreditamt,
case when fc.amtsourcedr>0 then (select sum(round(fc.amtsourcedr,2)) from fact_acct fc
where fc.record_id  =$P{inpGlJournalId}) else null end  as sumdebitamt,
usr.name as creator, sreg.name as dept, cal.C_Calendar_id,
'' User_manager,
'' sahebe_salahiya,
'' mudeer_idara,
'' Financial_Rep
from fin_reconciliation  recon join fact_acct fc on recon.FIN_Reconciliation_ID =fc.record_id
join fin_financial_account_acct acctcon on acctcon.fin_financial_account_id=recon.fin_financial_account_id
left join c_project p on p.C_Project_ID =fc.C_Project_ID
join FIN_Financial_Account finacct on finacct.Fin_Financial_Account_ID=recon.Fin_Financial_Account_ID
join AD_Org org on org.AD_Org_ID=recon.AD_Org_ID
join C_Calendar cal on cal.C_Calendar_id=org.C_Calendar_id
left join ad_user usr on usr.ad_user_id=recon.createdby
left join c_bpartner bp on bp.c_bpartner_id=usr.c_bpartner_id
left join c_salesregion sreg on sreg.c_salesregion_id=bp.em_efin_salesregion
where recon.fin_reconciliation_id  =$P{inpGlJournalId}
and recon.posted ='Y' and recon.ad_client_id =$P{inpClientId} and recon.documentno =$P{inpRequestNo}
group by recon.documentno, fc.datetrx, fc.em_efin_acctseq, recon.Statementdate, cal.C_Calendar_id,
p.name, recon.EM_Efin_Description, fc.amtsourcedr,fc.amtsourcecr, usr.name, sreg.name
order by creditamt limit 1)

union
(select tran.EM_Efin_Document_No, eut_convert_to_hijri(to_char(tran.dateacct,'YYYY-MM-DD')) as dateofjournal, fc.em_efin_acctseq as acctseq,
eut_convert_to_hijri(to_char(tran.Statementdate,'YYYY-MM-DD')) as Statementdate, pr.name as themonth, coalesce(yr.Description,yr.Year) as Descriptionyear,
p.name as project, concat('Payment Reversal of',' ',tran.description) as description,
round(fc.amtsourcecr,2) as creditamt,
(select sum(round(fc.amtacctcr,2)) from fact_acct fc
where fc.record_id  =$P{inpGlJournalId} and fc.docbasetype='PRJ' and fc.amtacctcr>'0')  as sumcreditamt,
case when fc.amtsourcedr>0 then  (select sum(round(fc.amtsourcedr,2)) from fact_acct fc
where fc.docbasetype='PRJ' and fc.record_id  = $P{inpGlJournalId} ) else null end  as sumdebitamt,
usr.name as creator, sreg.name as dept, cal.C_Calendar_id
, '' User_manager,
'' sahebe_salahiya,
'' mudeer_idara,
'' Financial_Rep
from FIN_Finacc_Transaction  tran join fact_acct fc on tran.FIN_Finacc_Transaction_ID =fc.record_id
left join c_project p on p.C_Project_ID =fc.C_Project_ID
join FIN_Financial_Account finacct on finacct.Fin_Financial_Account_ID=tran.Fin_Financial_Account_ID
join AD_Org org on org.AD_Org_ID=finacct.AD_Org_ID
join C_Calendar cal on cal.C_Calendar_id=org.C_Calendar_id
join c_year yr on yr.c_calendar_id=cal.c_calendar_id
join c_period pr on pr.c_year_id=yr.c_year_id
left join ad_user usr on usr.ad_user_id=tran.createdby
left join c_bpartner bp on bp.c_bpartner_id=usr.c_bpartner_id
left join c_salesregion sreg on sreg.c_salesregion_id=bp.em_efin_salesregion
where tran.FIN_Finacc_Transaction_ID  = $P{inpGlJournalId} and tran.Statementdate>=pr.StartDate and tran.Statementdate<=pr.EndDate
and tran.posted ='Y' and tran.ad_client_id =$P{inpClientId} and tran.EM_Efin_Document_No = $P{inpRequestNo}
group by tran.EM_Efin_Document_No, tran.dateacct, fc.em_efin_acctseq, tran.Statementdate, pr.name, Descriptionyear, cal.C_Calendar_id,
p.name, tran.description, fc.amtsourcedr,fc.amtsourcecr, usr.name, sreg.name
order by creditamt limit 1)

union
(select recon.documentno, eut_convert_to_hijri(to_char(fc.datetrx,'YYYY-MM-DD')) as dateofjournal, fc.em_efin_acctseq as acctseq,
eut_convert_to_hijri(to_char(recon.Statementdate,'YYYY-MM-DD')) as Statementdate,
(select prd.name from c_period prd
left join c_year yr on yr.c_year_id=prd.c_year_id
where CAST(recon.Statementdate as date) BETWEEN startdate and enddate and yr.C_Calendar_id=cal.C_Calendar_id) as themonth,
(select coalesce(yr.Description,yr.Year) from c_period prd
left join c_year yr on yr.c_year_id=prd.c_year_id
where CAST(recon.Statementdate as date) BETWEEN startdate and enddate and yr.C_Calendar_id=cal.C_Calendar_id) as Descriptionyear,
p.name as project, recon.EM_Efin_Description as description,
round(fc.amtsourcecr,2) as creditamt,
(select sum(round(fc.amtsourcecr,2)) from fact_acct fc
where fc.record_id  =$P{inpGlJournalId} and fc.amtsourcecr>'0')  as sumcreditamt,
case when fc.amtsourcedr>0 then (select sum(round(fc.amtsourcedr,2)) from fact_acct fc
where fc.record_id  =$P{inpGlJournalId}) else null end  as sumdebitamt,
usr.name as creator, sreg.name as dept, cal.C_Calendar_id ,
 '' User_manager,
 '' sahebe_salahiya,
'' mudeer_idara,
'' Financial_Rep
from fin_reconciliation  recon join fact_acct fc on recon.FIN_Reconciliation_ID =fc.record_id
join fin_financial_account_acct acctcon on acctcon.fin_financial_account_id=recon.fin_financial_account_id
left join c_project p on p.C_Project_ID =fc.C_Project_ID
join FIN_Financial_Account finacct on finacct.Fin_Financial_Account_ID=recon.Fin_Financial_Account_ID
join AD_Org org on org.AD_Org_ID=finacct.AD_Org_ID
join C_Calendar cal on cal.C_Calendar_id=org.C_Calendar_id
join c_year yr on yr.c_calendar_id=cal.c_calendar_id
join c_period pr on pr.c_year_id=yr.c_year_id
left join ad_user usr on usr.ad_user_id=recon.createdby
left join c_bpartner bp on bp.c_bpartner_id=usr.c_bpartner_id
left join c_salesregion sreg on sreg.c_salesregion_id=bp.em_efin_salesregion
where recon.fin_reconciliation_id  =$P{inpGlJournalId} and recon.Statementdate>=pr.StartDate and recon.Statementdate<=pr.EndDate
and recon.posted ='Y' and recon.Docstatus='EFIN_UREC' and recon.ad_client_id =$P{inpClientId} and recon.documentno =$P{inpRequestNo}
group by recon.documentno, fc.datetrx, fc.em_efin_acctseq, recon.Statementdate, cal.C_Calendar_id,
p.name, recon.EM_Efin_Description, fc.amtsourcedr,fc.amtsourcecr, usr.name, sreg.name
order by creditamt limit 1)

union
(select payin.em_efin_generalsequence as DocumentNo, eut_convert_to_hijri(to_char(fc.datetrx,'YYYY-MM-DD')) as dateofjournal, fc.em_efin_acctseq as acctseq,
eut_convert_to_hijri(to_char(payin.Paymentdate,'YYYY-MM-DD')) as Statementdate, pr.name as themonth, coalesce(yr.Description,yr.Year) as Descriptionyear,
p.name as project, fc.description as description,
round(fc.amtsourcecr,2) as creditamt,
(select sum(round(fc.amtsourcecr,2)) from fact_acct fc
where fc.em_efin_documentno =$P{inpRequestNo}  and fc.amtsourcecr>'0')  as sumcreditamt,
case when fc.amtsourcedr>0 then  (select sum(round(fc.amtsourcedr,2)) from fact_acct fc
where fc.em_efin_documentno =$P{inpRequestNo}) else null end  as sumdebitamt,
usr.name as creator, sreg.name as dept, cal.C_Calendar_id ,
'' User_manager,
'' sahebe_salahiya,
'' mudeer_idara,
'' Financial_Rep
from fin_payment  payin join fact_acct fc on payin.fin_payment_id =fc.record_id
left join c_project p on p.C_Project_ID =fc.C_Project_ID
join AD_Org org on org.AD_Org_ID=payin.AD_Org_ID
join C_Calendar cal on cal.C_Calendar_id=org.C_Calendar_id
join c_year yr on yr.c_calendar_id=cal.c_calendar_id
join c_period pr on pr.c_year_id=yr.c_year_id
left join ad_user usr on usr.ad_user_id=payin.createdby
left join c_bpartner bp on bp.c_bpartner_id=usr.c_bpartner_id
left join c_salesregion sreg on sreg.c_salesregion_id=bp.em_efin_salesregion
where payin.Paymentdate>=pr.StartDate and payin.Paymentdate<=pr.EndDate
and payin.Posted='Y' and fc.docbasetype ='ARR' and payin.ad_client_id =$P{inpClientId} and payin.em_efin_generalsequence =$P{inpRequestNo}
group by payin.em_efin_generalsequence, fc.datetrx, fc.em_efin_acctseq, payin.Paymentdate, pr.name, Descriptionyear, cal.C_Calendar_id,
p.name, fc.description, fc.amtsourcedr,fc.amtsourcecr, usr.name, sreg.name
order by creditamt limit 1)]]>
	</queryString>
