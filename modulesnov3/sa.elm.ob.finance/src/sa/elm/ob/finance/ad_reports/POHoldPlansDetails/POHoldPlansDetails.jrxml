	<queryString>
		<![CDATA[select
	e.value account,
	e.name acct_name,
	co.documentno || '    ' || coalesce(ph.description,'') contract_num,
	co.grandtotal contract_amt,
	co.em_efin_paid_amt paid_amt,
	co.em_efin_remaining_amt rem_amt,
	bp.value || '   ' || bp.name bp_name,
	co.EM_Escm_Reference_No reference_no,
	(
		select
			value || ' - ' || name
		from
			c_bpartner
		where
			c_bpartner_id = co.EM_Escm_Consultant
	) Consultant,
	(
		select
			proposalno
		from
			Escm_Proposalmgmt a
		where
			a.escm_proposalmgmt_id = co.EM_Escm_Proposalmgmt_ID
	) proposal_no,
	binq.org_amt,
	binq.current_budget,
	binq.funds_available,
	inv.documentno inv_number,
	rd.documentno rdv_num,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					inv.dateinvoiced,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) inv_date,
	rdt.txn_version || ' - ' || rdt.certificate_no txn_version,
	rdt.match_amt version_amt,
	inv.grandtotal inv_amt,
	phl.hold_amount,
	(
		select
			yr.year
		from
			c_year yr
		where
			yr.c_year_id = $P{C_Year_ID}
	) fin_year,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					co.DateOrdered,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) po_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					rdt.txnver_date,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) version_date,
	(
		select
			name
		from
			ad_user
		where
			ad_user_id = phl.createdby
	) user_name_line,
	(
		select
			name
		from
			ad_user
		where
			ad_user_id = ph.createdby
	) user_name_header,
	(
		select
			b.arabicname
		from
			efin_rdv_hold_types a,
			eut_deflookups_typeln b,
			eut_deflookups_type c
		where
			a.deductiontype = b.eut_deflookups_typeln_id
			and b.eut_deflookups_type_id = c.eut_deflookups_type_id
			and c.value = 'HOLD_TYPE'
			and a.efin_rdv_hold_types_id = phl.efin_rdv_hold_types_id limit 1
	) hold_type,
	phl.Description Descr_pohold_lines,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					phl.created,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) creation_date,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) as arabicdate,
	To_char(
		now(),
		'DD/MM/YYYY'
	) as gregoriandate
from
	EFIN_Rdv_BudgHold ph,
	EFIN_Rdv_BudgHoldLine phl,
	c_validcombination c,
	c_bpartner bp,
	efin_rdv rd,
(
		select
			min( trunc( startdate )) startdate,
			max( trunc( enddate )) enddate,
			c_year_id
		from
			c_period
		group by
			c_year_id
	) per,
	c_order co,
	efin_rdvtxn rdt
left join c_invoice inv on
	rdt.c_invoice_id = inv.c_invoice_id,
	c_elementvalue e
left join(
		select
			sum( org_amt ) org_amt,
			sum( binq.current_budget ) current_budget,
			sum( binq.funds_available ) funds_available,
			binq.c_elementvalue_id
		from
			efin_budgetinquiry binq,
			c_salesregion salereg,
			c_campaign compgn
		where
			binq.c_salesregion_id = salereg.c_salesregion_id
			and binq.c_campaign_id = compgn.c_campaign_id
			and salereg.value = '990'
			and compgn.value = '02'
		group by
			binq.c_elementvalue_id
	) binq on
	binq.c_elementvalue_id = e.c_elementvalue_id
where
	ph.efin_rdv_budghold_id = phl.efin_rdv_budghold_id
	and phl.c_validcombination_id = c.c_validcombination_id
	and c.account_id = e.c_elementvalue_id
	and ph.c_order_id = co.C_Order_ID
	and co.c_bpartner_id = bp.c_bpartner_id
	and ph.efin_rdvtxn_id = rdt.efin_rdvtxn_id
	and rdt.efin_rdv_id = rd.efin_rdv_id
	and trunc(rdt.txnver_date) between per.startdate and per.enddate
	and per.c_year_id = $P{C_Year_ID} 
	$P!{Aux_inpAcctId} 
	$P!{Aux_InpOrderId}
	and e.value between(
		select
			min( value )
		from
			c_elementvalue e
		where
			1 = 1 and(
				case
					when $P{ChapterFrom_Id} = '3' then e.value like '3%'
					when $P{ChapterFrom_Id} = '4' then e.value like '4%'
					else(
						e.value like '3%'
						or e.value like '4%'
					)
				end
			)
	)
	and(
		select
			max( value )
		from
			c_elementvalue e
		where
			1 = 1
			and(
				case
					when $P{ChapterTo_Id} = '3' then e.value like '3%'
					when $P{ChapterTo_Id} = '4' then e.value like '4%'
					else(
						e.value like '3%'
						or e.value like '4%'
					)
				end
			)
	)
order by
	e.value,
	co.documentno,
	rdt.txnver_date]]>
	</queryString>
