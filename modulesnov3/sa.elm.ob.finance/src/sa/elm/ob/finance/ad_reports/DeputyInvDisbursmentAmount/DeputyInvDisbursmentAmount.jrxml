								(
									select
										multiplyrate
									from
										c_conversion_rate
									where
										c_currency_id = inv.c_currency_id
								),
								1
							)
						end
					) )  as actualamount,
			EM_Efin_C_Elementvalue_ID --inv.documentno
		from
			c_invoiceline ln
		join c_invoice inv on
			inv.c_invoice_id = ln.c_invoice_id
		join Efin_Budgetint bint on
			inv.EM_Efin_Budgetint_ID = bint.efin_budgetint_id
	--	join efin_purchasein_app_hist hist on inv.c_invoice_id = hist.c_invoice_id	
	    join (select  distinct  c_invoice_id,trunc(created) created,seqno from efin_purchasein_app_hist a) hist on inv.c_invoice_id = hist.c_invoice_id
		join (select c_invoice_id,max( seqno ) seqno	from efin_purchasein_app_hist h	group by c_invoice_id) hist1 on  hist.c_invoice_id = hist1.c_invoice_id and hist.seqno = hist1.seqno							
		where
			date_part(
				'month',
				hist.created
			)= 11
			and inv.docstatus = 'CO'
			and  bint.c_year_id=  $P{C_Year_ID} 
		group by
			--inv.documentno,
 EM_Efin_C_Elementvalue_ID
	) novinvln on
	novinvln.EM_Efin_C_Elementvalue_ID = lines.c_elementvalue_id --- December
left join(
		select
			'December' as janmonth,
			sum( linenetamt *(
						case
							when inv.c_currency_id = '317' then 1
							else coalesce(
								(
									select
										multiplyrate
									from
										c_conversion_rate
									where
										c_currency_id = inv.c_currency_id
								),
								1
							)
						end
					) )  as actualamount,
			EM_Efin_C_Elementvalue_ID --inv.documentno
		from
			c_invoiceline ln
		join c_invoice inv on
			inv.c_invoice_id = ln.c_invoice_id
		join Efin_Budgetint bint on
			inv.EM_Efin_Budgetint_ID = bint.efin_budgetint_id
	--	join efin_purchasein_app_hist hist on inv.c_invoice_id = hist.c_invoice_id
        join (select  distinct  c_invoice_id,trunc(created) created,seqno from efin_purchasein_app_hist a) hist on inv.c_invoice_id = hist.c_invoice_id	
		join (select c_invoice_id,max( seqno ) seqno	from efin_purchasein_app_hist h	group by c_invoice_id) hist1 on  hist.c_invoice_id = hist1.c_invoice_id and hist.seqno = hist1.seqno								
		where
			date_part(
				'month',
				hist.created
			)= 12
			and inv.docstatus = 'CO'
			and  bint.c_year_id=  $P{C_Year_ID} 
		group by
			--inv.documentno,
 EM_Efin_C_Elementvalue_ID
	) decinvln on
	decinvln.EM_Efin_C_Elementvalue_ID = lines.c_elementvalue_id --total
left join(
		select
			'Total' as janmonth,
			sum( linenetamt *(
						case
							when inv.c_currency_id = '317' then 1
							else coalesce(
								(
									select
										multiplyrate
									from
										c_conversion_rate
									where
										c_currency_id = inv.c_currency_id
								),
								1
							)
						end
					) )  as actualamount,
			EM_Efin_C_Elementvalue_ID --inv.documentno
		from
			c_invoiceline ln
		join c_invoice inv on
			inv.c_invoice_id = ln.c_invoice_id
		join Efin_Budgetint bint on
			inv.EM_Efin_Budgetint_ID = bint.efin_budgetint_id
	--	join efin_purchasein_app_hist hist on inv.c_invoice_id = hist.c_invoice_id	
	    join (select  distinct  c_invoice_id,trunc(created) created,seqno from efin_purchasein_app_hist a) hist on inv.c_invoice_id = hist.c_invoice_id
		join (select c_invoice_id,max( seqno ) seqno	from efin_purchasein_app_hist h	group by c_invoice_id) hist1 on  hist.c_invoice_id = hist1.c_invoice_id and hist.seqno = hist1.seqno	
		where
			 inv.docstatus = 'CO'
			and  bint.c_year_id=  $P{C_Year_ID} 
		group by
			--inv.documentno,
 EM_Efin_C_Elementvalue_ID
	) totalinvln on
	totalinvln.EM_Efin_C_Elementvalue_ID = lines.c_elementvalue_id
join c_elementvalue act on
	act.c_elementvalue_id = lines.c_elementvalue_id
where
	lines.c_campaign_id in(
		'11F9670AC2DC499885BAE97494C595AD',
		'0'
	)
	 and act.accounttype ='E'
	 and (case
			when   $P{Chapter_ID}  = '3' then (act.value like  '30%' or act.value like  '37%')
			when    $P{Chapter_ID} = '4' then  act.value like '4%'
			else  
			(      act.value like  '30%' 
			    or act.value like  '37%'  
				or act.value like '4%')
		end 		
		)
	 $P!{Aux_acct} 
order by
	element]]>
