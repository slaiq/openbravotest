				c_doctype ty
			where
				c_doctype_id = inv.C_DocTypeTarget_ID
		)= 'Y' then coalesce(inv.description,'  قيمة المستخلص الجاري رقم ' ||coalesce((
			select
				coalesce(
					rdt.certificate_no,
					to_char(rdt.txn_version)
