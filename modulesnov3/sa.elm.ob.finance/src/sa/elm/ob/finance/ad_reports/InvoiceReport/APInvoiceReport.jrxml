				)|| ' الموافق ' || To_char(
					rdt.Txnver_Date,
					'DD/MM/YYYY'
				) version_date_greg
			from
				efin_rdvtxn rdt
			where
				rdt.c_invoice_id = inv.c_invoice_id limit 1
		),'')|| ' ' ||coalesce((
			select
			trim( regexp_replace( co.EM_Escm_Notes, '\r|\n', ' ', 'g' ))		
			from
				c_order co
			where
				co.c_order_id = inv.C_order_id limit 1
		),'')|| ' برقم تبليغ  ' ||coalesce(coalesce((
		select
--				to_char(coalesce(leg.award_no,''))
                to_char(leg.award_no)
			from
				Escm_Legacy_Contract leg,
				c_order co
			where
				co.em_escm_legacycontract = leg.escm_legacy_contract_id
				and  co.c_order_id =coalesce(inv.c_order_id,inv.EM_Efin_C_Order_ID)
		),(select EM_Escm_Letter_No from c_order c2 where c2.c_order_id = coalesce(inv.c_order_id,inv.EM_Efin_C_Order_ID) )),''))
		else inv.description
	end description
,
