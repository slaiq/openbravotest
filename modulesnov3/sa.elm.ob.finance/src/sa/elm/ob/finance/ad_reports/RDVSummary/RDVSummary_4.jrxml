	<parameter name="Efin_Rdv_ID" class="java.lang.String"/>
	<parameter name="TXN_Type" class="java.lang.String"/>
	<parameter name="Version_Id" class="java.lang.String"/>
	<parameter name="p_legacy_contract_num" class="java.lang.String"/>
	<parameter name="p_adv_deduct" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select  distinct
	rd.documentno rdv_number,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					rd.txn_date,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) Transaction_Date,
	co.documentno PO_number,
	bg.internalno internal_num,
	(
		select
			b.bankname || ' / ' || coalesce(
				addr.address1,
				''
			) Bank_name_addr
		from
			efin_bank b
		left join efin_address addr on
			b.efin_bank_id = addr.efin_bank_id
		where
			b.efin_bank_id = rd.efin_bank_id
	) Bank_address,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					bg.Expirydateh,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) Expiry_date,
	(
		select
			e.name
		from
			c_elementvalue e,
			c_validcombination v
		where
			e.c_elementvalue_id = v.account_id
			and v.c_validcombination_id = co.em_efin_c_validcombination_id
	) account_name,
	bp.name bp_name,
	bg.Document_Amount,
	(
		select
			sum( coalesce( rdt.ADV_Deduct, 0 )) ADV_Deduct
		from
			efin_rdvtxn rdt
		where
			rdt.efin_rdv_id = rd.efin_rdv_id
			and rdt.isadvancetransaction = 'N'
	) AA,		
	(
		select
			sum( coalesce( rdt.match_amt, 0 ))
		from
			efin_rdvtxn rdt
		where
			rdt.efin_rdv_id = rd.efin_rdv_id
			and rdt.isadvancetransaction = 'N'
	) AD,
	abs(( select sum( coalesce( rdt.ADV_Deduct, 0 )) ADV_Deduct from efin_rdvtxn rdt where rdt.efin_rdv_id = rd.efin_rdv_id and rdt.isadvancetransaction = 'N' )-( select sum( coalesce( rdt.match_amt, 0 )) ADV_Deduct from efin_rdvtxn rdt where rdt.efin_rdv_id = rd.efin_rdv_id and rdt.isadvancetransaction = 'N' )) AE,
	(
		select
			c.name
		from
			ad_role a,
			ad_user_roles b,
			ad_user c
		where
			a.ad_role_id = b.ad_role_id
			and b.ad_user_id = c.ad_user_id
			and b.isactive = 'Y'
			and c.isactive = 'Y'
			and c.ad_user_id <> '100'
--			and a.name like 'مدير عام الشؤون المالية'
			and a.ad_role_id='2E78C8B91DC84BDABC02F1FA081BFDA6'
	) 	Financial_Rep,
--(select name from ad_user where username ='700053' limit 1)	Financial_Rep,
-- coalesce(co.EM_Escm_Legacy_Adv_Payment_Amt,0)+coalesce((select sum(a.Netmatch_Amt) from efin_rdvtxn a where a.efin_rdv_id=rd.efin_rdv_id and isadvancetransaction = 'Y') ,0)  adv_inv_amt,
coalesce(rd.Legacy_Totaladv_Paid,0)+coalesce((select sum(a.Netmatch_Amt) from efin_rdvtxn a where a.efin_rdv_id=rd.efin_rdv_id and isadvancetransaction = 'Y') ,0) adv_inv_amt,
rd.Legacy_Advance_Balance-coalesce((select sum(a.adv_deduct) from efin_rdvtxn a where a.efin_rdv_id=rd.efin_rdv_id and isadvancetransaction = 'N') ,0) open_adv_bal,
	ABS(TRUNC((
		select
			sum( coalesce( rdt.ADV_Deduct, 0 )) ADV_Deduct
		from
			efin_rdvtxn rdt
		where
			rdt.efin_rdv_id = rd.efin_rdv_id
			and rdt.isadvancetransaction = 'N'
	)  - (
		select
			coalesce(
				sum( to_number( invl1.linenetamt )),
				0
			)
		from
			c_invoiceline invl1,
			efin_rdvtxn rdt1
		where
			rdt1.c_invoice_id = invl1.c_invoice_id
			and rdt1.efin_rdv_id =  rd.efin_rdv_id  -- '535376FDBA6C4BAC9B652A65ECF8FCE2' 
			and invl1.EM_Efin_Istax = 'N'
			and rdt1.isadvancetransaction = 'Y'
			and invl1.linenetamt > 0
	))) AE1,
--coalesce (co.EM_Escm_Legacy_Adv_Deduction,0)
coalesce (co.EM_Escm_L_Opening_Adv_Balance,0)  Adv_Deduc_Legacy
from
	efin_rdv rd,
	efin_rdvtxn rdt,
    c_bpartner bp,
	c_order co
left join escm_bankguarantee_detail bg on
	bg.c_order_id = co.c_order_id
where
	rd.c_order_id = co.c_order_id 
	and rd.efin_rdv_id = rdt.efin_rdv_id
	and rd.c_bpartner_id = bp.c_bpartner_id
--	and rd.documentno = '390000079'
   and rdt.efin_rdvtxn_id =   $P{Version_Id} 
  and rd.efin_rdv_id=  $P{Efin_Rdv_ID}
  and  rd.txn_type = $P{TXN_Type}]]>
	</queryString>
