from (select round(sum(fc.amtsourcedr),2) as debitamt,nullif(floor(sum(fc.amtsourcedr)),'0') as decdebit,
case when fc.amtsourcedr>0 then (select sum(fc.amtsourcedr) from fact_acct fc join
fin_payment  payin on payin.fin_payment_id =fc.record_id
join c_elementvalue e on e.c_elementvalue_id=fc.account_id
where fc.em_efin_documentno =$P{inpGlJournalId} and fc.docbasetype ='ARR' and fc.ad_client_id =$P{inpClientId}  and fc.amtsourcedr>0) else null end  as sumdebitamt,
e.value as natacct, p.value as project, e.name||'-'||p.name||'-'||fc.description as wholedesc,
(select efin_getparentelementlist(fc.ad_client_id, e.c_elementvalue_id)) as subacct
