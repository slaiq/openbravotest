	<parameter name="inpClientId" class="java.lang.String"/>
	<queryString>
		<![CDATA[select debitdecilist, debitintlist, sumdebitint, sumdecidebit, wholedesc, project, naturalacct
from
((select array_to_string(array_agg(decdebit),E'\r\n') as debitdecilist,
replace(array_to_string(array_agg(' '||floor((debitamt*100)-(decdebit*100))),CHR(10)),' 0',' 00') as debitintlist,
to_char((array_agg(floor(sumdebitamt)))[1]) as sumdebitint,
(array_agg(replace(to_char(floor(((sumdebitamt)-(floor(sumdebitamt)))*100)) ,'0','00')))[1] as sumdecidebit,
array_to_string(array_agg(wholedesc),'') as wholedesc,
array_to_string(array_agg(subacct),E'\r\n') as project,
array_to_string(array_agg(natacct),E'\r\n') as naturalacct
from (select round(gll.amtsourcedr,2) as debitamt,nullif(floor(gll.amtsourcedr),'0') as decdebit,
case when gll.amtsourcedr>0 then (select sum(gll.amtsourcedr) from gl_journalline gll join C_ValidCombination vc on
vc.C_ValidCombination_ID=gll.C_ValidCombination_ID join c_elementvalue e on e.c_elementvalue_id=vc.account_id
where gll.gl_journal_id  =$P{inpGlJournalId} and gl.ad_client_id =$P{inpClientId} and gll.amtsourcedr>0) else null end  as sumdebitamt,
e.value as natacct, p.value as project, e.name ||'-'|| p.name||'-'|| gll.description as wholedesc,
(select efin_getparentelementlist(gl.ad_client_id, e.c_elementvalue_id)) as subacct
from  gl_journal gl
join gl_journalline  gll on gll.gl_journal_id=gl.gl_journal_id
join c_project p on p.C_Project_ID =gll.C_Project_ID
left join c_elementvalue e on e.c_elementvalue_id=gll.em_efin_account
where gl.gl_journal_id  =$P{inpGlJournalId} and gl.ad_client_id =$P{inpClientId} and gll.amtsourcedr>0) debit)

union all
(select array_to_string(array_agg(decdebit),E'\r\n') as debitdecilist,
replace(array_to_string(array_agg(' '||floor((debitamt*100)-(decdebit*100))),CHR(10)),' 0',' 00') as debitintlist,
to_char((array_agg(floor(sumdebitamt)))[1]) as sumdebitint,
(array_agg(replace(to_char(floor(((sumdebitamt)-(floor(sumdebitamt)))*100)) ,'0','00')))[1] as sumdecidebit,
array_to_string(array_agg(wholedesc),'') as wholedesc,
array_to_string(array_agg(subacct),E'\r\n') as project,
array_to_string(array_agg(natacct),E'\r\n') as naturalacct
from (select round(fc.amtsourcedr,2) as debitamt,nullif(floor(fc.amtsourcedr),'0') as decdebit,
case when fc.amtsourcedr>0 then (select sum(fc.amtsourcedr) from fact_acct fc join
c_invoice inv on inv.c_invoice_id =fc.record_id
join c_elementvalue e on e.c_elementvalue_id=fc.account_id
where fc.record_id  =$P{inpGlJournalId}  and inv.ad_client_id=$P{inpClientId}  and fc.amtsourcedr>0) else null end  as sumdebitamt,
e.value as natacct, p.value as project,
e.name||'-'||p.name||'  '|| (select array_to_string(array_agg(description),E'\r\n') from c_invoice where c_invoice_id=inv.c_invoice_id) as wholedesc,
(select efin_getparentelementlist(inv.ad_client_id, e.c_elementvalue_id)) as subacct
from fact_acct fc
join c_invoice  inv on inv.c_invoice_id=fc.record_id
left join c_elementvalue e on e.c_elementvalue_id=fc.account_id
join c_project p on p.C_Project_ID =fc.C_Project_ID
where fc.record_id  =$P{inpGlJournalId}  and inv.ad_client_id =$P{inpClientId} and fc.amtsourcedr>0) debit)

union all
(select array_to_string(array_agg(decdebit),E'\r\n') as debitdecilist,
replace(array_to_string(array_agg(' '||floor((debitamt*100)-(decdebit*100))),CHR(10)),' 0',' 00') as debitintlist,
