array_to_string(array_agg(subacct),E'\r\n') as project,
array_to_string(array_agg(natacct),E'\r\n') as naturalacct
from (select round(fc.amtsourcedr,2) as debitamt,nullif(floor(fc.amtsourcedr),'0') as decdebit,
case when fc.amtsourcedr>0 then (select sum(fc.amtsourcedr) from fact_acct fc join
fin_reconciliation recon on recon.fin_reconciliation_id =fc.record_id
join c_elementvalue e on e.c_elementvalue_id=fc.account_id
where fc.record_id  =$P{inpGlJournalId}  and recon.Docstatus='CO' and recon.ad_client_id =$P{inpClientId} and fc.amtsourcedr>0) else null end  as sumdebitamt,
e.value as natacct, p.value as project, e.name ||'-'||p.name ||'-'||recon.em_efin_description as wholedesc,
(select efin_getparentelementlist(recon.ad_client_id, e.c_elementvalue_id)) as subacct
from  fact_acct fc
join fin_reconciliation recon on recon.fin_reconciliation_id=fc.record_id
left join c_project p on p.C_Project_ID =fc.C_Project_ID
left join c_elementvalue e on e.c_elementvalue_id=fc.account_id
where fc.record_id  =$P{inpGlJournalId}  and recon.Docstatus='CO' and recon.ad_client_id =$P{inpClientId}  and fc.amtsourcedr>0) debit)

union all
