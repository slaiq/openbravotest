			binq.REV_Amount orig_budget,
			binq.Funds_Available,
			--	Case when salereg.value ='999' then binq.current_budget else 0 end current_budget,
 --	Case when salereg.value ='990' then binq.REV_Amount else 0 end orig_budget,
 --   binq.Encumbrance,
(
				 select sum(ab.inv_amt) from 
				(select
					sum( invl.linenetamt )
					 *(
						case
							when inv.c_currency_id = '317' then 1
							else coalesce(
								(
									select
										multiplyrate
									from
										c_conversion_rate
									where
										c_currency_id = inv.c_currency_id
								),
								1
							)
						end
					)  inv_amt
				from
					c_invoice inv,
					c_invoiceline invl
				where
					inv.c_invoice_id = invl.c_invoice_id
					and invl.EM_Efin_C_Elementvalue_ID = e.c_elementvalue_id --	and inv.documentno = '400000225'
					and inv.docstatus = 'EFIN_WFA'
					and inv.em_efin_budgetint_id= bint.efin_budgetint_id
					group by inv.c_currency_id) ab
			) Encumbrance,
			(
				 select sum(ab.inv_amt) from 
				(select
					sum( invl.linenetamt )*(
						case
							when inv.c_currency_id = '317' then 1
							else coalesce(
								(
									select
										multiplyrate
									from
										c_conversion_rate
									where
										c_currency_id = inv.c_currency_id
								),
								1
							)
						end
					)  inv_amt
				from
					c_invoice inv,
					c_invoiceline invl
				where
					inv.c_invoice_id = invl.c_invoice_id
					and invl.EM_Efin_C_Elementvalue_ID = e.c_elementvalue_id --	and inv.documentno = '400000225'
					and inv.docstatus = 'CO'
					and inv.Ispaid = 'Y'
					and inv.posted = 'N' 
					and inv.em_efin_budgetint_id= bint.efin_budgetint_id
					group by inv.c_currency_id) ab
			) payment_completed,
			(
				 select sum(ab.inv_amt) from 
				(select
					sum( invl.linenetamt ) 
					*(
						case
							when inv.c_currency_id = '317' then 1
							else coalesce(
								(
									select
										multiplyrate
									from
