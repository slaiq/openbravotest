			) pmt_not_completed,
			--		binq.Spent_Amt act_amount,
(
				 select sum(ab.inv_amt) from 
				(select
					sum( invl.linenetamt )
					*(
						case
							when inv.c_currency_id = '317' then 1
							else coalesce(
								(
									select
										multiplyrate
									from
										c_conversion_rate
									where
