				from
					c_invoice inv,
					c_invoiceline invl
				where
					inv.c_invoice_id = invl.c_invoice_id
					and invl.EM_Efin_C_Elementvalue_ID = e.c_elementvalue_id --	and inv.documentno = '400000225'
					and inv.docstatus = 'DR'
					and inv.em_efin_budgetint_id= bint.efin_budgetint_id
					group by inv.c_currency_id) ab
			) draft_amt,
			yr.year,
			bint.efin_budgetint_id,
			vc.account_id,
			binq.c_campaign_id,
			binq.ad_client_id,
			binq.ad_org_id,
			e.c_elementvalue_id,
			E.VALUE account,
			e.name,
			e.em_efin_classification_AR account_classification,
			camp.name || ' - ' || camp.value budget_type,
			camp.value,
			(
				case
					when e.value like '21%' then '1'
					when e.value like '22%' then '2'
					when e.value like '23%' then '2'
					when e.value like '24%' then '2'
					when e.value like '25%' then '2'
					when e.value like '26%' then '2'
					when e.value like '27%' then '2'
					when e.value like '28%' then '2'
					when e.value like '3%'  then '3'
					when e.value like '4%'  then '4'
				end
			) Chapter
		from
			c_elementvalue e,
			efin_budgetinquiry binq,
			c_salesregion salereg,
			c_year yr,
			efin_budgetint bint,
			c_validcombination vc,
			c_campaign camp
		where
			bint.c_year_id = yr.c_year_id
			and bint.efin_budgetint_id = binq.efin_budgetint_id
			and binq.c_elementvalue_id = e.c_elementvalue_id
			and binq.c_salesregion_id = salereg.c_salesregion_id
			and e.ad_client_id = yr.ad_client_id
			and binq.c_validcombination_id = vc.c_validcombination_id
			and binq.c_campaign_id = camp.c_campaign_id
			and e.accounttype = 'E'
			and e.isactive ='Y'
			and salereg.value in('990') -- ,'999')
			$P!{AuxUSER_CLIENT} --	and E.VALUE like '21%' 
			and(
				case
					when $P{Chapter_ID} = '1' then E.VALUE like '21%'
					when $P{Chapter_ID} = '2' then(
						E.VALUE like '22%'
						or E.VALUE like '23%'
						or E.VALUE like '24%'
						or E.VALUE like '25%'
						or E.VALUE like '26%'
						or E.VALUE like '27%'
						or E.VALUE like '28%'
					)
					when $P{Chapter_ID} = '3' then E.VALUE like '3%'
					when $P{Chapter_ID} = '4' then E.VALUE like '4%' --	else E.VALUE like '%'
					else E.VALUE like '21%'
					or E.VALUE like '22%'
					or E.VALUE like '23%'
					or E.VALUE like '24%'
					or E.VALUE like '25%'
					or E.VALUE like '26%'
					or E.VALUE like '27%'
					or E.VALUE like '28%'
					or E.VALUE like '3%'
					or E.VALUE like '4%'
				end
			) 
			$P!{AUXinpYear} 
			$P!{AUXBudType}
		group by
			yr.year,
			bint.efin_budgetint_id,
			vc.account_id,
			binq.c_campaign_id,
			binq.ad_client_id,
			binq.ad_org_id,
			e.c_elementvalue_id,
			binq.c_elementvalue_id,
			E.VALUE,
			e.name,
			e.em_efin_classification,
			camp.name,
			camp.value,
			binq.REV_Amount,
			binq.Funds_Available,
			salereg.value,
			binq.Encumbrance,
			binq.Spent_Amt,
			binq.current_budget
	) as curr
order by
	curr.account,
	curr.value]]>
