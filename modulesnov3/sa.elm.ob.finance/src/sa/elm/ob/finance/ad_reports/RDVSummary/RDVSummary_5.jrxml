	<parameter name="Efin_Rdv_ID" class="java.lang.String"/>
	<parameter name="P_F" class="java.math.BigDecimal"/>
	<parameter name="TXN_Type" class="java.lang.String"/>
	<parameter name="Version_Id" class="java.lang.String"/>
	<parameter name="p_a2_5" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select
case when rd.contract_amt =(select sum(Match_Amt) from efin_rdvtxn txn where txn.efin_rdv_id = rd.efin_rdv_id
									and txn.txn_version <= rdt.txn_version
									and txn.isadvancetransaction ='N'		)
		then 'الختامي'
		else 'الجاري'
		end Field_name,
	bp.name,
	rd.Documentno rdv_number,
	co.EM_Escm_Notes description,
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					rdt.Txnver_Date,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) versio_date_hijri,
	sum( inv.GrandTotal ) inv_amount,
	inv.documentno invoice_number,	
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					inv.dateinvoiced,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) inv_Date,
	(
		select
			(
				case
					when e.value like '21%' then '1'
					when e.value like '22%' then '2'
					when e.value like '23%' then '2'
					when e.value like '24%' then '2'
					when e.value like '25%' then '2'
					when e.value like '26%' then '2'
					when e.value like '27%' then '2'
					when e.value like '28%' then '2'
					when e.value like '3%' then '3'
					when e.value like '4%' then '4'
				end
			)|| '/' || e.value
		from
			c_elementvalue e,
			c_validcombination v
		where
			e.c_elementvalue_id = v.account_id
			and v.c_validcombination_id = co.em_efin_c_validcombination_id
	) account_number,
	cy."year",
	to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) as ArabicDate,
	To_char(
		now(),
		'DD/MM/YYYY'
	) as GregorianDate
from
	efin_rdv rd,
	c_bpartner bp,
	c_order co,
	Efin_RDVTxn rdt,
	c_invoice inv,
	efin_budgetint ebd,
	c_year cy
where
	rd.C_Bpartner_ID = bp.C_Bpartner_ID
	and rd.c_order_id = co.c_order_id
	and rdt.efin_rdv_id = rd.efin_rdv_id
	and rdt.c_invoice_id = inv.c_invoice_id
	and ebd.efin_budgetint_id = inv.em_efin_budgetint_id
	and cy.c_year_id = ebd.c_year_id
	--and rdt.isadvancetransaction = 'N'
--	and rd.documentno = '390000079'
    and  rd.efin_rdv_id= $P{Efin_Rdv_ID} 
    and  rd.txn_type = $P{TXN_Type} 
	and rdt.efin_rdvtxn_id =   $P{Version_Id} 
    --  'A9D42AC4A25A420CAE0F25FC168D2335'
group by
	bp.name,
	rd.Documentno,
	co.EM_Escm_Notes,
	rdt.Txnver_Date,
	inv.documentno,
	inv.dateinvoiced,
	co.em_efin_c_validcombination_id,
	cy."year"
		,
	rd.contract_amt,
	rd.efin_rdv_id,rdt.txn_version]]>
	</queryString>
