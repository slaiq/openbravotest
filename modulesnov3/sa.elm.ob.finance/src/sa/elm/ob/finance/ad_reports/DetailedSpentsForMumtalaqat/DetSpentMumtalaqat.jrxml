	<queryString language="SQL">
		<![CDATA[select
	full_account,
	account,
	 account_name,
	count(inv_num) inv_num_cnt,
	inv_num,
	inv_date,
	description,
	sum( Debit ) dr_amt,
	sum( Credit ) cr_amt,
	bp_name,
	Yawmiya_no,
	Taswiya_no,
	hijri_date,
	gregoriandate
from
	(
		
	select
			e.value full_account,
			'      ' || Substr(
				e.value,
				1,
				1
			)|| '  ' || Substr(
				e.value,
				2
			)|| '       ' || e.NAME account,
			 e.NAME account_name,
			inv.documentno inv_num,
		/*	Eut_convert_to_hijri(
				To_char(
					trunc(inv.dateinvoiced),
					'YYYY-MM-DD'
				)
			) */
			to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					inv.dateinvoiced,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	)  inv_date,
			trunc(inv.dateinvoiced) gl_date,
			inv.Description,
			(
				case
					when sign( invl.LineNetAmt )= 1 then invl.LineNetAmt
					else 0
				end
			) Debit,
			abs( case when sign( invl.LineNetAmt )=- 1 then invl.LineNetAmt else 0 end ) Credit,
			(
				select
					trim( name )
				from
					c_bpartner
				where
					c_bpartner_id = inv.c_bpartner_id
			) bp_name,
			(
				select
					EM_Efin_Acct_Seq
				from
					fin_payment pay,
					FIN_Finacc_Transaction fintran
				where
					fintran.fin_payment_id = pay.fin_payment_id
					and pay.em_efin_invoice_id = inv.c_invoice_id
					and fintran.STATUS != 'EFIN_CAN' limit 1
			) Yawmiya_no,
			to_char('') Taswiya_no,
			to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) as hijri_date,
			To_char(
					now(),
				'DD/MM/YYYY'
				) as  GregorianDate,
			inv.ad_client_id
		from
			c_elementvalue e,
			c_validcombination c,
			c_invoice inv,
			c_invoiceline invl
		where
			c.account_id = e.c_elementvalue_id
			and inv.c_invoice_id = invl.c_invoice_id
			and c.c_validcombination_id = invl.em_efin_c_validcombination_id
		and inv.EM_Efin_C_Salesregion_ID ='1B477717BFE044E48C240D705A97AFE4'
--			and inv.documentno='400004126'
		--	and e.value in ('411960000','401020000','405601002','412800160')
			and e.accounttype = 'E'
		--	and inv.docstatus = 'CO'
		--	and inv.ispaid = 'Y' 		
           and ( inv.description like  '%'|| $P{inv_description} || '%'  or $P{inv_description} is null )
           and (Case when  $P{C_Elementvalue_Id_From} is not null then 
           	        e.value in ('411960000','401020000','405601002','412800160')
           	 --     e.value like '%'
           	        else e.value like '%'    end       ) 
	union all
	
	select
			e.value full_account,
			'      ' || Substr(
				e.value,
				1,
				1
			)|| '  ' || Substr(
				e.value,
				2
			)|| '  ' || e.NAME account,
			 e.NAME account_name,
			to_char('') inv_num,
		/*	Eut_convert_to_hijri(
				To_char(
					gl.datedoc,
					'YYYY-MM-DD'
				)
			) */
			to_char(
		to_Date(
			Eut_convert_to_hijri(
				To_char(
					gl.datedoc,
					'YYYY-MM-DD'
				)
			)
		),
		'DD/MM/YYYY'
	) Inv_date,
			trunc(gl.datedoc) gl_date,
			gl.description,
			gll.AmtSourceDr Debit,
			gll.AmtSourceCr Credit,
			(
				select
					name
				from
					C_Bpartner
				where
					C_Bpartner_ID = gl.C_Bpartner_ID
			) bp_name,
			fa.em_efin_acctseq Yawmiya_no,
			gl.documentno Taswiya_no,
			Eut_convert_to_hijri(
				To_char(
					now(),
					'YYYY-MM-DD'
				)
			) as hijri_date,
			To_char(
				now(),
				'DD-MM-YYYY'
			) as GregorianDate,
			gl.ad_client_id
		from
			GL_Journal gl,
			GL_JournalLine gll,
			c_elementvalue e,
			fact_acct fa
		where
			gl.gl_journal_id = gll.gl_journal_id
			and fa.line_id = gll.gl_journalline_id  
			and gll.em_efin_account = e.c_elementvalue_id 
	--		and e.c_elementvalue_id = fa.account_id
			and fa.em_efin_acctseq is not null
			and e.accounttype = 'E'
			  and ( gl.description like  '%'|| $P{inv_description} || '%'  or $P{inv_description} is null )		
			   and (Case when  $P{C_Elementvalue_Id_From} is not null then 
           	        e.value in ('411960000','401020000','405601002','412800160')
           	        else e.value like '%'    end       ) 	
			
	) main
where
	1 = 1
	--and  main.full_account in ('411960000','401020000','405601002','412800160')
 and (
				case
					when $P{Chapter_ID} = '1' then main.full_account like '21%'
					when $P{Chapter_ID} = '2' then(
						main.full_account like '22%'
						or main.full_account like '23%'
						or main.full_account like '24%'
						or main.full_account like '25%'
						or main.full_account like '26%'
						or main.full_account like '27%'
						or main.full_account like '28%'
					)
					when $P{Chapter_ID} = '3' then main.full_account like '3%'
					when $P{Chapter_ID} = '4' then main.full_account like '4%'
					else main.full_account like '%'
				end
			)
 $P!{AuxUSER_CLIENT} 	
  $P!{AUX_inpInvDate} 
   $P!{AUXinpAcctID} 
group by
	full_account,
	account,
	inv_num,
	inv_date,
	description,
	bp_name,
	Yawmiya_no,
	hijri_date,
	gregoriandate,
	Taswiya_no,
	account_name
order by
	main.full_account,
	main.inv_num,
	main.inv_date,
	main.Taswiya_no]]>
	</queryString>
