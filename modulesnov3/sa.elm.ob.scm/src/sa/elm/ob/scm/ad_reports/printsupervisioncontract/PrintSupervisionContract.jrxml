	<queryString>
		<![CDATA[select em_escm_projectname, (SELECT EXTRACT(DOW FROM  ord.em_escm_signaturedate::date)::int) as signatureday,
eut_convert_to_hijri(to_char(em_escm_signaturedate,'YYYY-MM-dd')) as em_escm_signaturedate,
to_char(em_escm_signaturedate, 'dd-MM-yyyy') as signaturedategreg, cty.name as contsigncity, em_escm_motcontperson, bp.name as vendorname, bpcert.certificatenumber as registryno,
bpcert.registrydate, bpcert.registrycity, loc.address2 as postalbox, loc.postal as postalcode,case when em_escm_periodtype='DT' then 'يوم' when em_escm_periodtype='MT' then 'شهر'
when em_escm_periodtype='MTH' then 'شهر(هـ)' when em_escm_periodtype='MTG' then 'شهر(م)' end as periodtype,
(select usr.name from ad_user usr where usr.c_bpartner_id= bp.c_bpartner_id order by usr.created limit 1) as vndrcontactname,
(select usr.title from ad_user usr where usr.c_bpartner_id= bp.c_bpartner_id order by usr.created limit 1) as vndrcontactposition,
(select em_escm_authorization_number from ad_user where c_bpartner_id = bp.c_bpartner_id order by created limit 1) as vndrcontauthno,
(select eut_convert_to_hijri(to_char(em_escm_authorization_date,'YYYY-MM-dd')) from ad_user
where c_bpartner_id = bp.c_bpartner_id order by created limit 1) as vndrcontauthdate,
em_escm_contractduration, grandtotal as conttotamt,  em_escm_motcontperson, em_escm_motcontposition, bid.bidpurpose as projectpurpose
from c_order ord
left join c_bpartner bp on bp.c_bpartner_id=ord.c_bpartner_id
left join (select c_location_id, c_bpartner_id from c_bpartner_location where created=(select min(created) as created from c_bpartner_location
where c_bpartner_id=(select c_bpartner_id from c_order where c_order_id=$P{inpPOId}))) bploc on bploc.c_bpartner_id=bp.c_bpartner_id
left join c_location loc on loc.c_location_id=bploc.c_location_id
left join c_city cty on cty.c_city_id=ord.em_escm_c_city_id
left join (select name, value, escm_deflookups_typeln_id, cert.c_bpartner_id, cert.certificatenumber,
eut_convert_to_hijri(to_char(cert.registrydate,'YYYY-MM-dd')) as registrydate, cert.registrycity from escm_deflookups_typeln lkln
join escm_deflookups_type typ on lkln.escm_deflookups_type_id=typ.escm_deflookups_type_id
right join escm_certificates cert on cert.certificatename=lkln.escm_deflookups_typeln_id
where reference ='CN' and typ.isactive='Y' and (value ilike '%CRN%')) bpcert on bpcert.c_bpartner_id=bp.c_bpartner_id
left join escm_bidmgmt bid on bid.escm_bidmgmt_id=ord.em_escm_bidmgmt_id
where ord.c_order_id=$P{inpPOId}]]>
	</queryString>
