	<queryString>
		<![CDATA[select ord.em_escm_projectname as projectname, bg.bankname, bg.bankbgno as bankrefltrnum, bg.bgstartdateh as bankrefltrdate, bg.bgamount, bg.expirydateh,
dist.acctbudgno, dist.acctbudgname, bploc.name as vndraddr, loc.address2 as vndr2line, bploc.phone as vndrphoneno,
cty.name as contsigncity, (SELECT EXTRACT(DOW FROM  ord.em_escm_signaturedate::date)::int) as signatureday,
eut_convert_to_hijri(to_char(ord.em_escm_signaturedate,'YYYY-MM-dd')) as em_escm_signaturedate,
ord.em_escm_motcontperson as contactperson, bp.name as vndrname, bpcert.certificatenumber as commercialregno,
bpcert.registrydate as commercialregdate, bpcert.registrycity as commercialregcity, loc.postal as vndrpostalcode,
(select usr.name from ad_user usr where usr.c_bpartner_id= bp.c_bpartner_id order by usr.created limit 1) as vndrcontactname,
(select usr.title from ad_user usr where usr.c_bpartner_id= bp.c_bpartner_id order by usr.created limit 1) as vndrcontactposition,
(select em_escm_authorization_number from ad_user where c_bpartner_id = bp.c_bpartner_id order by created limit 1) as vndrcontauthno,
(select eut_convert_to_hijri(to_char(em_escm_authorization_date,'YYYY-MM-dd')) from ad_user where c_bpartner_id = bp.c_bpartner_id order by created limit 1) as vndrcontauthdate,
ord.em_escm_contractduration as prjduration, ord.grandtotal as conttotalamt, ord.em_escm_motcontposition as contactpersonposition
from c_order ord
left join (select bgdtl.c_order_id, string_agg(bnk.bankname, ', ') as bankname, string_agg(CAST(coalesce(bgdtl.bgamount, 0) as text), ', ') as bgamount,
string_agg(bgdtl.bankbgno, ', ') as bankbgno, string_agg(eut_convert_to_hijri(to_char(bgdtl.bgstartdateh,'YYYY-MM-dd')), ', ') as bgstartdateh,
string_agg(eut_convert_to_hijri(to_char(bgdtl.expirydateh,'YYYY-MM-dd')), ', ') as expirydateh
from escm_bankguarantee_detail bgdtl left join escm_bgworkbench bgwb on bgwb.escm_bgworkbench_id=bgdtl.escm_bgworkbench_id
left join efin_bank bnk on bnk.efin_bank_id=bgdtl.bankname group by bgdtl.c_order_id, bgwb.escm_bgworkbench_id) bg
on bg.c_order_id=ord.c_order_id
left join (select string_agg(accountno, ', ') as acctbudgno, string_agg(budgetprojname, ', ') as acctbudgname, c_order_id from escm_pgmt_distribution
group by c_order_id) dist on dist.c_order_id=ord.c_order_id
left join c_bpartner_location bploc on bploc.c_bpartner_location_id=ord.c_bpartner_location_id
left join c_location loc on loc.c_location_id=bploc.c_location_id
left join c_city cty on cty.c_city_id=ord.em_escm_c_city_id
left join c_bpartner bp on bp.c_bpartner_id=ord.c_bpartner_id
left join (select name, value, escm_deflookups_typeln_id, cert.c_bpartner_id, cert.certificatenumber,
eut_convert_to_hijri(to_char(cert.registrydate,'YYYY-MM-dd')) as registrydate, cert.registrycity from escm_deflookups_typeln lkln
join escm_deflookups_type typ on lkln.escm_deflookups_type_id=typ.escm_deflookups_type_id
right join escm_certificates cert on cert.certificatename=lkln.escm_deflookups_typeln_id
where reference ='CN' and typ.isactive='Y' and (name ilike '%Commercial Registry%'
or escm_deflookups_typeln_id='E1A43F37E4FD41DC95530AB0DD6ED707')) as bpcert on bpcert.c_bpartner_id=bp.c_bpartner_id
where ord.c_order_id=$P{inpPOId}]]>
	</queryString>
