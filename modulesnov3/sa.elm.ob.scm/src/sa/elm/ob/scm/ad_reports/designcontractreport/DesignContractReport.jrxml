				<text><![CDATA[أولاً : الدراسة والتصميم:]]></text>
			</staticText>
			<staticText>
				<reportElement x="0" y="420" width="555" height="20" uuid="e56b2e85-b2d3-4a01-88ac-cd31f4ec56b9"/>
