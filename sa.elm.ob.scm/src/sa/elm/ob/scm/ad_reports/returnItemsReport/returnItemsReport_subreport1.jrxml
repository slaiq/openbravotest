	<property name="ireport.y" value="0"/>
	<parameter name="inpInOutId" class="java.lang.String"/>
	<queryString>
		<![CDATA[select rec.itemcode, rec.itemdesc, rec.uomcode, rec.em_escm_inspdecision, rec.movementQty, alllns.sno as seqno, rec.count
from
(select prd.value as itemcode, coalesce(inoln.description, '') as itemdesc, coalesce(uomtrl.name, uom.name) as uomcode,em_escm_inspdecision,
inoln.movementQty, row_number() OVER ()::integer as rnum,
(select count(m_inoutline_id) from m_inoutline where m_inout_id=inoln.m_inout_id) as count
from m_inoutline inoln
left join m_product prd on inoln.m_product_id=prd.m_product_id
left join c_uom uom on uom.c_uom_id=inoln.c_uom_id
left join c_uom_trl uomtrl on uomtrl.c_uom_id=uom.c_uom_id and uomtrl.ad_language  ='ar_SA'
where inoln.m_inout_id=$P{inpInOutId})rec
full join ( select ''::character varying as ad_client_id, b.sno
from (select generate_series(1, (select ceil(((select count(m_inoutline_id)
from m_inoutline where m_inout_id=$P{inpInOutId}))::numeric / 10) * 10):: integer) as sno)b ) alllns on rec.rnum = alllns.sno
order by alllns.sno]]>
	</queryString>
