	<queryString>
		<![CDATA[select einfo.ehcm_emp_perinfo_id, ement.employeeno, einfo.arabicfullname as fullname,
einfo.nationality_identifier as natid, nat.name as natname,
org.name as empdeptname, qual.degree as quallification,
einfo.townbirth, eut_convert_to_hijri(to_char(einfo.dob,'YYYY-MM-dd')) as dob, addr.address1, cont.name as country, einfo.mobno, einfo.email,
ement.jobtitle, (coalesce(grd.value, '')||'-'||coalesce(ement.jobtitle, '')||'-'||coalesce(ement.jobno, '')) as jobdtl,  grd.value as grade,
grdstp.point as gradestep, eut_convert_to_hijri(to_char(einfo.hiredate,'YYYY-MM-dd')) as hiredate,
eut_convert_to_hijri(to_char(einfo.startdate,'YYYY-MM-dd')) as joindate,
bs.amount as basicsal, transall.value as transportallow,
eut_convert_to_hijri(to_char(now(),'YYYY-MM-DD')) as today, lev.annualleave, lev.leave, lev.balance,
(img.binarydata) as profpic
from ehcm_emp_perinfo einfo
left join (select ehcm_emp_perinfo_id, employeeno, jobtitle, pos.mcs_letter_no as jobno, empifo.ehcm_position_id, employmentgrade, ehcm_payscaleline_id from ehcm_employment_info empifo left join ehcm_position pos on pos.ehcm_position_id=empifo.ehcm_position_id
where empifo.startdate = (select max(startdate) from ehcm_employment_info where ehcm_emp_perinfo_id =$P{inpEmpId})
and ehcm_emp_perinfo_id  =$P{inpEmpId}) ement on einfo.ehcm_emp_perinfo_id=ement.ehcm_emp_perinfo_id
left join ehcm_position dept on dept.ehcm_position_id=ement.ehcm_position_id
left join ad_org org on org.ad_org_id=dept.department_id
left join ehcm_addnationality nat on nat.ehcm_addnationality_id=einfo.ehcm_addnationality_id
left join c_country cont on cont.c_country_id=einfo.c_country_id
left join ehcm_emp_address addr on addr.ehcm_emp_perinfo_id=einfo.ehcm_emp_perinfo_id
left join (select degree, ehcm_emp_perinfo_id from ehcm_qualification
where startdate=(select max(startdate) from ehcm_qualification where ehcm_emp_perinfo_id=$P{inpEmpId})
and ehcm_emp_perinfo_id=$P{inpEmpId}) qual on qual.ehcm_emp_perinfo_id=einfo.ehcm_emp_perinfo_id
left join ehcm_grade grd on grd.ehcm_grade_id=ement.employmentgrade
left join ehcm_payscaleline bs on bs.ehcm_payscaleline_id=ement.ehcm_payscaleline_id
left join ehcm_progressionpt grdstp on grdstp.ehcm_progressionpt_id=bs.ehcm_progressionpt_id
left join (select rateln.value, rateln.ehcm_grade_id
from ehcm_payroll_report_config prconf
left join ehcm_graderates rate on rate.ehcm_graderates_id=prconf.ehcm_graderates_id
left join ehcm_graderatelines rateln on rateln.ehcm_graderates_id=rate.ehcm_graderates_id
where prconf.ad_client_id=$P{inpClientId}) transall on transall.ehcm_grade_id=ement.employmentgrade
left join (select abacc.days as annualleave, acc.ehcm_emp_perinfo_id,
(select p_availedleavedays from ehcm_getavailed_availablelev($P{inpEmpId}, $P{inpEffectiveDate}, null,
(select ehcm_absence_type_id from ehcm_payroll_report_config where ad_client_id=$P{inpClientId}), $P{inpClientId}, 1, 'CR', null, '1', null)) as leave,
(select  p_availableleavedays  from ehcm_getavailed_availablelev($P{inpEmpId}, $P{inpEffectiveDate}, null,
(select ehcm_absence_type_id from ehcm_payroll_report_config where ad_client_id=$P{inpClientId}),
$P{inpClientId}, 1, 'CR', null, '1', null)) as balance
from ehcm_payroll_report_config prconf
left join ehcm_absence_type abtyp on abtyp.ehcm_absence_type_id=prconf.ehcm_absence_type_id
left join ehcm_absencetype_accruals abacc on abacc.ehcm_absence_type_id=abtyp.ehcm_absence_type_id
left join ehcm_absence_accrualdetails acdts on acdts.ehcm_absence_type_id=prconf.ehcm_absence_type_id
left join ehcm_absence_accrual acc on acc.ehcm_absence_accrual_id=acdts.ehcm_absence_accrual_id
where CAST($P{inpEffectiveDate} as date) BETWEEN abacc.startdate and coalesce(abacc.enddate, cast(now() as date))
and acc.ehcm_emp_perinfo_id=$P{inpEmpId}) lev on lev.ehcm_emp_perinfo_id=einfo.ehcm_emp_perinfo_id
left join ad_image img on img.ad_image_id=einfo.work_ad_image_id
where einfo.ehcm_emp_perinfo_id =$P{inpEmpId} $P!{Aux_inpDepartmentId}]]>
	</queryString>
