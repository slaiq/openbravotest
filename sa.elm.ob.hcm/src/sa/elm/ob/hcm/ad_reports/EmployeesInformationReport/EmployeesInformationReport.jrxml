	<queryString>
		<![CDATA[select txnempinfo.ehcm_emp_perinfo_id,
emp.value as empno,emp.arabicfullname as empname,empinfo.changereason,
eut_convert_to_hijri(to_char(txnempinfo.startdate,'YYYY-MM-DD')) as startdate,
n.name as nationality,pos.job_no as position,eut_convert_to_hijri(to_char(emp.govhiredate,'YYYY-MM-DD')) as govhiredate,
eut_convert_to_hijri(to_char(emp.hiredate,'YYYY-MM-DD')) as hiredate,coalesce(trl.name,list.name) as qualification,
eut_convert_to_hijri(to_char(emp.Dob,'YYYY-MM-DD')) as dob,coalesce(statuslist.name,status.name) as empstatus,
ehcm_calculate_age(emp.ehcm_emp_perinfo_id,emp.ad_client_id,to_char(emp.dob,'YYYY-MM-DD'),to_char(now(),'YYYY-MM-DD')) as age,
ehcm_calculate_age(emp.ehcm_emp_perinfo_id,emp.ad_client_id,to_char(emp.startdate,'YYYY-MM-DD'),
to_char(coalesce(empinfo.enddate,now()),'YYYY-MM-DD')) as period,
grcl.name as empcategory,org.name as dept,emp.nationality_identifier as nationalidentifer,grcl.name as empcategory,gr.name as grade,
jobs.job_title as title,(jobs.value || concat('-',jobs.job_title)) as positiontitle,
case when emp.gender='M' then 'Male' when emp.gender='F' then 'Female' end as gender,payscln.amount as basicsalary,qual.degree as degree,
superemp.arabicfullname as linemanager,
case when $P{inpEmpStatus}='T' then eut_convert_to_hijri(to_char(txnempinfo.startdate,'YYYY-MM-DD')) else '' end as endofservice,
coalesce(decbal.period_of_service,0) as period_of_service

from
(
(select empinfoh.ehcm_emp_perinfo_id, empinfoh.startdate
from ehcm_employment_info empinfoh where empinfoh.ad_client_id=$P{inpClientId} and
case when $P{inpEmpStatus} = 'H' then  empinfoh.changereason = 'H'
when $P{inpEmpStatus} = 'PR' then  empinfoh.changereason = 'PR'
when $P{inpEmpStatus} = 'PRT' then  empinfoh.changereason = 'PRT'
when $P{inpEmpStatus} = 'T' then  (empinfoh.changereason = 'T' or (empinfoh.changereason='SUE' and empinfoh.ehcm_emp_suspension_id is not null and
empinfoh.ehcm_emp_suspension_id in (select sus.ehcm_emp_suspension_id from ehcm_emp_suspension sus
left join ehcm_suspension_reason susrs on susrs.ehcm_suspension_reason_id=sus.suspension_end_reason
where susrs.value in ('T','TRD') and sus.ehcm_emp_suspension_id=empinfoh.ehcm_emp_suspension_id)))
else empinfoh.changereason in ('H','PR','PRT','SUS','SUE','T') end
and empinfoh.startdate between cast($P{inpStartDate} as date) and cast($P{inpEndDate} as date))
union

(select bmis.ehcm_emp_perinfo_id, bsum.startdate
from ehcm_emp_businessmission bmis
join ehcm_busmission_summary bsum on bsum.ehcm_emp_businessmission_id=bmis.ehcm_emp_businessmission_id
where decision_status='I' and bmis.ad_client_id=$P{inpClientId} and $P{inpEmpStatus}='BM'
and bsum.startdate between cast($P{inpStartDate} as date) and cast($P{inpEndDate} as date))
union

(select abattn.ehcm_emp_perinfo_id, abattn.startdate
from ehcm_absence_attendance abattn
where abattn.ehcm_absence_attendance_id in (with recursive rel_tree(ehcm_absence_attendance_id, level, path_info) as (
   select ehcm_absence_attendance_id,
   1 as level, ehcm_absence_attendance_id::text as path_info
   from ehcm_absence_attendance
   where original_decision_no is null and ad_client_id= $P{inpClientId} and decision_status='I'
   union all
   select ch.ehcm_absence_attendance_id,
   par.level + 1, par.path_info
   from ehcm_absence_attendance ch
   join rel_tree par on ch.original_decision_no = par.ehcm_absence_attendance_id and ch.decision_status='I'
)
select ehcm_absence_attendance_id
from rel_tree where level=(select max(level) from rel_tree e where e.path_info=rel_tree.path_info group by path_info)
order by path_info, level) and decision_type<>'CA' and abattn.decision_status='I' and $P{inpEmpStatus}='ABS'
and abattn.ad_client_id= $P{inpClientId}
and abattn.startdate between cast($P{inpStartDate} as date) and cast($P{inpEndDate} as date)
order by abattn.decision_no)
union

(select sch.ehcm_emp_perinfo_id, sch.startdate
from ehcm_emp_scholarship sch
where sch.ehcm_emp_scholarship_id in (with recursive rel_tree(ehcm_emp_scholarship_id, level, path_info) as (
   select ehcm_emp_scholarship_id,
   1 as level, ehcm_emp_scholarship_id::text as path_info
   from ehcm_emp_scholarship
   where original_decision_no is null and ad_client_id= $P{inpClientId} and decision_status='I'
   union all
   select ch.ehcm_emp_scholarship_id,
   par.level + 1, par.path_info
   from ehcm_emp_scholarship ch
   join rel_tree par on ch.original_decision_no = par.ehcm_emp_scholarship_id and ch.decision_status='I'
)
select ehcm_emp_scholarship_id
from rel_tree where level=(select max(level) from rel_tree e where e.path_info=rel_tree.path_info group by path_info)
order by path_info, level) and decision_type<>'CA' and sch.decision_status='I' and $P{inpEmpStatus}='SCTR'
and sch.ad_client_id= $P{inpClientId}
and sch.startdate between cast($P{inpStartDate} as date) and cast($P{inpEndDate} as date)
order by sch.decision_no)) txnempinfo left join ehcm_emp_perinfo emp on emp.ehcm_emp_perinfo_id=txnempinfo.ehcm_emp_perinfo_id
left join (select empinfo.changereason,empinfo.ehcm_emp_perinfo_id,empinfo.ehcm_position_id,empinfo.deptcode,empinfo.employmentgrade,empinfo.jobcode,
empinfo.ehcm_payscaleline_id,empinfo.EHCM_Emp_Supervisor_id,empinfo.enddate
from ehcm_employment_info empinfo where empinfo.created =
(select max(info.created) from ehcm_employment_info info where info.ehcm_emp_perinfo_id = empinfo.ehcm_emp_perinfo_id))
empinfo on emp.ehcm_emp_perinfo_id = empinfo.ehcm_emp_perinfo_id

left join ehcm_position pos on pos.ehcm_position_id = empinfo.ehcm_position_id
left join ehcm_gradeclass grcl on grcl.ehcm_gradeclass_id = emp.ehcm_gradeclass_id
left join ad_org org on org.ad_org_id = empinfo.deptcode
left join ehcm_grade gr on gr.ehcm_grade_id=empinfo.employmentgrade
left join Ehcm_jobs jobs on jobs.ehcm_jobs_id = empinfo.jobcode
left join ad_ref_list listst on listst.value=emp.employment_status and listst.ad_reference_id='BD5BC7FE25C64B158971A7DCE5732DE9'
left join AD_Ref_List_Trl trlst on listst.ad_ref_list_id=trlst.ad_ref_list_id and trlst.ad_language='ar_SA'
left join ehcm_addnationality n on n.ehcm_addnationality_id = emp.ehcm_addnationality_id
left join ehcm_qualification qual on qual.ehcm_emp_perinfo_id = emp.ehcm_emp_perinfo_id
left join ad_ref_list list on list.value=qual.edulevel and list.ad_reference_id='3FA605387FD5432BB9F334A3CB7C4965'
left join AD_Ref_List_Trl trl on list.ad_ref_list_id=trl.ad_ref_list_id and trl.ad_language='ar_SA'
left join ehcm_payscaleline payscln on payscln.ehcm_payscaleline_id=empinfo.ehcm_payscaleline_id
left join EHCM_Emp_Supervisor superv on superv.EHCM_Emp_Supervisor_id = empinfo.EHCM_Emp_Supervisor_id
left join ehcm_emp_perinfo superemp on superemp.ehcm_emp_perinfo_id = superv.ehcm_emp_perinfo_id
left join ehcm_employeestatus_v status on status.ehcm_employeestatus_v_id= emp.ehcm_emp_perinfo_id
left join AD_Ref_List_Trl statuslist on statuslist.ad_ref_list_id= status.ad_ref_list_id and statuslist.ad_language='ar_SA'
left join ( select coalesce(decbal.period_of_service,0) as period_of_service, decbal.ehcm_emp_perinfo_id  from ehcm_decision_balance decbal join ehcm_deci_bal_hdr decbalhdr on decbalhdr.ehcm_deci_bal_hdr_id= decbal.ehcm_deci_bal_hdr_id
where decbalhdr.status='CO' and  decbal.decision_type='TPS')decbal on decbal. ehcm_emp_perinfo_id= emp.ehcm_emp_perinfo_id

where emp.ad_client_id =  $P{inpClientId} and txnempinfo.startdate between cast($P{inpStartDate} as date) and cast($P{inpEndDate} as date)
and case when $P{inpEmpId} is null then true else emp.ehcm_emp_perinfo_id=$P{inpEmpId} end
and case when $P{inpDepartmentId} is null then true else empinfo.deptcode=$P{inpDepartmentId} end
and case when $P{inpEmpTypeId} is null then true else emp.ehcm_gradeclass_id=$P{inpEmpTypeId} end
and case when $P{inpEmpStatus} is null then true else status.statusvalue=$P{inpEmpStatus} end
and case when $P{inpGenderId} is null then true else emp.gender=$P{inpGenderId} end
and case when $P{inpLineManagerId} is null then true else empinfo.EHCM_Emp_Supervisor_id = $P{inpLineManagerId} end
and case when ($P{inpEmpGradeFromId}='0'  and $P{inpEmpGradeToId}='0') then true else true end
and case when ($P{inpEmpGradeFromId}='0' and $P{inpEmpGradeToId}!='0') then gr.value<=$P{inpEmpGradeToId} else true end
and case when ($P{inpEmpGradeFromId}!='0' and $P{inpEmpGradeToId}='0') then gr.value>=$P{inpEmpGradeFromId} else true end
and case when ($P{inpEmpGradeFromId}!='0' and $P{inpEmpGradeToId}!='0') then(gr.value>=$P{inpEmpGradeFromId}
and gr.value<=$P{inpEmpGradeToId}) else true end order by empno asc]]>
	</queryString>
